   <m:mn>2</m:mn>
  </m:msqrt>
  <m:mi mathvariant="normal">cis</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mfrac>
     <m:mrow>
      <m:mn>7</m:mn><m:mi>π</m:mi></m:mrow>
     <m:mn>8</m:mn>
    </m:mfrac>
    </m:mrow>
  <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mn>2</m:mn><m:msqrt>
   <m:mn>2</m:mn>
  </m:msqrt>
  <m:mi mathvariant="normal">cis</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mfrac>
     <m:mrow>
      <m:mn>15</m:mn><m:mi>π</m:mi></m:mrow>
     <m:mn>8</m:mn>
    </m:mfrac>
    </m:mrow>
  <m:mo>)</m:mo></m:mrow></m:mrow>
</m:math>
</para>
</solution>
</exercise>

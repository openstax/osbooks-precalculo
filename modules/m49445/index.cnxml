   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:msub>
       <m:mrow/>
       <m:mn>1</m:mn>
      </m:msub>
      <m:mo>=</m:mo><m:mfrac>
       <m:mn>1</m:mn>
       <m:mn>6</m:mn>
      </m:mfrac>
      <m:mo>;</m:mo><m:mo> </m:mo><m:msub>
       <m:mi>a</m:mi>
       <m:mi>n</m:mi>
      </m:msub>
      <m:mo>=</m:mo><m:msub>
       <m:mi>a</m:mi>
       <m:mrow>
        <m:mi>n</m:mi><m:mo>–</m:mo><m:mn>1</m:mn>
       </m:mrow>
      </m:msub>
      <m:mo>−</m:mo><m:mfrac>

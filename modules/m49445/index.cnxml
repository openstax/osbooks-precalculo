        <m:mi>n</m:mi><m:mo>–</m:mo><m:mn>1</m:mn>
       </m:mrow>
      </m:msub>
      <m:mo>+</m:mo><m:mn>9</m:mn>
     </m:mrow>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mi>n</m:mi><m:mo>≥</m:mo><m:mn>2 </m:mn>
     </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable>
 </m:mrow>

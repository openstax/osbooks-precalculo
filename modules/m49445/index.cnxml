</entry>
    <entry> <m:math><m:mtable><m:mtr><m:mtd><m:msub><m:mi>a</m:mi><m:mi>n</m:mi></m:msub><m:mo>=</m:mo><m:msub><m:mi>a</m:mi><m:mn>1</m:mn></m:msub><m:mo>+</m:mo><m:mo>(</m:mo><m:mi>n</m:mi><m:mo>−</m:mo><m:mn>1</m:mn><m:mo>)</m:mo><m:mi>d</m:mi></m:mtd></m:mtr><m:mtr><m:mtd><m:msub><m:mi>a</m:mi><m:mn>11</m:mn></m:msub><m:mo>=</m:mo><m:mn>9</m:mn><m:mo>+</m:mo><m:mo>(</m:mo><m:mn>11</m:mn><m:mo> </m:mo><m:mn>1</m:mn><m:mo>)</m:mo><m:mo>(</m:mo><m:mn>5</m:mn><m:mo>)</m:mo></m:mtd></m:mtr><m:mtr><m:mtd><m:msub><m:mi>a</m:mi><m:mn>11</m:mn></m:msub><m:mo>=</m:mo><m:mn>9</m:mn><m:mo>+</m:mo><m:mo>(</m:mo><m:mn>10</m:mn><m:mo>)</m:mo><m:mo>(</m:mo><m:mn>5</m:mn><m:mo>)</m:mo></m:mtd></m:mtr><m:mtr><m:mtd><m:msub><m:mi>a</m:mi><m:mn>11</m:mn></m:msub><m:mo>=</m:mo><m:mn>59</m:mn></m:mtd></m:mtr><m:mtr><m:mtd><m:mspace/></m:mtd></m:mtr></m:mtable></m:math> </entry>
  </row>

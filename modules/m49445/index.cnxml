<exercise id="fs-id1165137540888"><problem id="fs-id1165137540891">
<para id="fs-id1165137863388"> <m:math>
 <m:mrow>
  <m:mi>a</m:mi>
  <m:mo>=</m:mo><m:mo>{</m:mo><m:mn>15,8</m:mn><m:mo>,</m:mo><m:mn>18,5</m:mn><m:mo>,</m:mo><m:mn>21,2</m:mn><m:mo>,</m:mo><m:mn>...</m:mn><m:mo>}</m:mo>

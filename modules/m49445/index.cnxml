    <m:mtd columnalign="left">
     <m:mrow>
      <m:msub>
       <m:mi>a</m:mi>
       <m:mn>1</m:mn>
      </m:msub>
      <m:mo>=</m:mo><m:mn>17</m:mn><m:mo>;</m:mo><m:mo> </m:mo><m:msub>
       <m:mi>a</m:mi>
       <m:mi>n</m:mi>
      </m:msub>
      <m:mo>=</m:mo><m:msub>
       <m:mi>a</m:mi>

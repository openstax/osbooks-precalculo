</m:math> y <m:math>
 <m:mrow>
  <m:msub>
   <m:mi>a</m:mi>
   <m:mi>n</m:mi>
  </m:msub>
  <m:mo>=</m:mo><m:mn>2 </m:mn><m:mo>+</m:mo><m:mn>20,4</m:mn><m:mi>n.</m:mi>
 </m:mrow>
</m:math> </para></solution>
</exercise>


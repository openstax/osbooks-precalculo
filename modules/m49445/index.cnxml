 <m:mrow>
  <m:mi>u</m:mi><m:mo stretchy="false">(</m:mo><m:mi>n</m:mi><m:mo stretchy="false">)</m:mo><m:mtext>?</m:mtext>
 </m:mrow>
</m:math> </para></problem>
<solution id="fs-id1165137834422">
<para id="fs-id1165137834424"> <m:math>
 <m:mrow>
  <m:mn>1</m:mn><m:mo>,</m:mo><m:mn>4</m:mn><m:mo>,</m:mo><m:mn>7</m:mn><m:mo>,</m:mo><m:mn>10</m:mn><m:mo>,</m:mo><m:mn>13</m:mn><m:mo>,</m:mo><m:mn>16</m:mn><m:mo>,</m:mo><m:mn>19</m:mn>
 </m:mrow>
</m:math> </para>
</solution>
</exercise>


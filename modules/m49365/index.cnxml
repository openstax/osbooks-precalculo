</m:mrow>
<m:mi>b</m:mi>
</m:msub>
<m:mrow><m:mo>(</m:mo>
<m:mrow>
<m:mn>7</m:mn><m:mi>x</m:mi><m:mo>⋅</m:mo><m:mn>2</m:mn><m:mi>y</m:mi>
</m:mrow>
<m:mo>)</m:mo></m:mrow>
</m:mrow>
</m:math> </para>
</problem>
<solution id="fs-id1165137843718">
<para id="fs-id1165137843721"> <m:math>
<m:mrow>
<m:msub>
<m:mrow>
<m:mi>log</m:mi>
</m:mrow>
<m:mi>b</m:mi>
</m:msub>
<m:mrow><m:mo>(</m:mo>
<m:mn>2 </m:mn>
<m:mo>)</m:mo></m:mrow><m:mo>+</m:mo><m:msub>
<m:mrow>
<m:mi>log</m:mi>
</m:mrow>
<m:mi>b</m:mi>
</m:msub>
<m:mrow><m:mo>(</m:mo>
<m:mn>7</m:mn>
<m:mo>)</m:mo></m:mrow><m:mo>+</m:mo><m:msub>
<m:mrow>
<m:mi>log</m:mi>
</m:mrow>
<m:mi>b</m:mi>
</m:msub>
<m:mrow><m:mo>(</m:mo>
<m:mi>x</m:mi>
<m:mo>)</m:mo></m:mrow><m:mo>+</m:mo><m:msub>
<m:mrow>
<m:mi>log</m:mi>
</m:mrow>
<m:mi>b</m:mi>
</m:msub>
<m:mrow><m:mo>(</m:mo>
<m:mi>y</m:mi>
<m:mo>)</m:mo></m:mrow>

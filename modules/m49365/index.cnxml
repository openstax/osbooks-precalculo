</solution>
</exercise>

<exercise id="fs-id1165134182094">
<problem id="fs-id1165134182096">
<para id="fs-id1165134182098"> <m:math>
<m:mrow>
<m:mi>ln</m:mi><m:mrow><m:mo>(</m:mo>
<m:mrow>
<m:mn>3</m:mn><m:mi>a</m:mi><m:mi>b</m:mi><m:mo>⋅</m:mo><m:mn>5</m:mn><m:mi>c</m:mi>
</m:mrow>
<m:mo>)</m:mo></m:mrow>
</m:mrow>
</m:math> </para>
</problem>
</exercise>

<exercise id="fs-id1165134182135">
<problem id="fs-id1165134182137">
<para id="fs-id1165134182139"> <m:math>
<m:mrow>
<m:msub>
<m:mrow>
<m:mi>log</m:mi>
</m:mrow>
<m:mi>b</m:mi>
</m:msub>
<m:mrow><m:mo>(</m:mo>
<m:mrow>
<m:mfrac>
<m:mrow>

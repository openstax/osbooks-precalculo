<note id="fs-id1165135635237" class="precalculus try">
<label>Ejercicio</label>
<exercise id="ti_04_05_09">
<problem id="fs-id1165135635245">
<para id="fs-id1165135635247">Expanda <m:math>
<m:mrow>
<m:mi>ln</m:mi><m:mrow><m:mo>(</m:mo>
<m:mrow>
<m:mfrac>
<m:mrow>
<m:msqrt>
<m:mrow>
<m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo>–</m:mo><m:mn>1</m:mn><m:mo stretchy="false">)</m:mo><m:msup>
<m:mrow>
<m:mo stretchy="false">(</m:mo><m:mn>2 </m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn><m:mo stretchy="false">)</m:mo>
</m:mrow>
<m:mn>2 </m:mn>
</m:msup>

</m:mrow>
</m:msqrt>

</m:mrow>
<m:mrow>
<m:mo stretchy="false">(</m:mo><m:msup>
<m:mi>x</m:mi>
<m:mn>2 </m:mn>
</m:msup>
<m:mo>-</m:mo><m:mn>9</m:mn><m:mo stretchy="false">)</m:mo>
</m:mrow>
</m:mfrac>

</m:mrow>
<m:mo>)</m:mo></m:mrow><m:mo>.</m:mo>
</m:mrow>
</m:math> </para>
</problem>
<solution id="fs-id1165135155467">
<para id="fs-id1165137725387"> <m:math>
<m:mrow>
<m:mfrac>
<m:mn>1</m:mn>
<m:mn>2</m:mn>
</m:mfrac>
<m:mi>ln</m:mi><m:mrow><m:mo>(</m:mo>
<m:mrow>
<m:mi>x</m:mi><m:mo>–</m:mo><m:mn>1</m:mn>
</m:mrow>
<m:mo>)</m:mo></m:mrow><m:mo>+</m:mo><m:mi>ln</m:mi><m:mrow><m:mo>(</m:mo>
<m:mrow>
<m:mn>2 </m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
</m:mrow>
<m:mo>)</m:mo></m:mrow><m:mo>-</m:mo><m:mi>ln</m:mi><m:mrow><m:mo>(</m:mo>
<m:mrow>
<m:mi>x</m:mi><m:mo>+</m:mo><m:mn>3</m:mn>
</m:mrow>
<m:mo>)</m:mo></m:mrow><m:mo>-</m:mo><m:mi>ln</m:mi><m:mrow><m:mo>(</m:mo>
<m:mrow>
<m:mi>x</m:mi><m:mo>-</m:mo><m:mn>3</m:mn>
</m:mrow>
<m:mo>)</m:mo></m:mrow>
</m:mrow>

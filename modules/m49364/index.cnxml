<m:mrow>
<m:mi>log</m:mi>
</m:mrow>
<m:mn>5</m:mn>
</m:msub>
<m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo>-</m:mo><m:mn>2 </m:mn><m:mo stretchy="false">)</m:mo><m:mo>+</m:mo><m:mn>1</m:mn><m:mo>?</m:mo>
</m:mrow>
</m:math> </para>
</problem>

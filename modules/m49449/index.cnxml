        <m:mi>n</m:mi><m:mo>&#8722;</m:mo><m:mi>k</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
       </m:mrow>
      <m:mo>)</m:mo></m:mrow><m:mi>n</m:mi><m:mo>!</m:mo><m:mo>+</m:mo><m:mi>k</m:mi><m:mi>n</m:mi><m:mo>!</m:mo>
     </m:mrow>
     <m:mrow>
      <m:mi>k</m:mi><m:mo>!</m:mo><m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mi>n</m:mi><m:mo>&#8722;</m:mo><m:mi>k</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
       </m:mrow>
      <m:mo>)</m:mo></m:mrow><m:mo>!</m:mo>
     </m:mrow>
    </m:mfrac>

   </m:mtd>
  </m:mtr>
  <m:mtr>
   <m:mtd>
    <m:mo>=</m:mo><m:mfrac>
     <m:mrow>
      <m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mi>n</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
       </m:mrow>
      <m:mo>)</m:mo></m:mrow><m:mi>n</m:mi><m:mo>!</m:mo>
     </m:mrow>
     <m:mrow>
      <m:mi>k</m:mi><m:mo>!</m:mo><m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mi>n</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
         </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>&#8722;</m:mo><m:mi>k</m:mi>

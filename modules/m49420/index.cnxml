    <m:mtd columnalign="left">
     <m:mrow>
      <m:mspace width="0.5em"/><m:mn>3</m:mn><m:mi>x</m:mi><m:mn>−2</m:mn><m:mi>y</m:mi><m:mo>=</m:mo><m:mn>5</m:mn>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mn>−9</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>6</m:mn><m:mi>y</m:mi><m:mo>=</m:mo><m:mn>−15</m:mn>
     </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable>
 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id1165133290327"><para id="fs-id1165133290328">Dependiente con infinitas soluciones</para></solution></exercise>

</section>


</m:math> y <m:math>
 <m:mrow>
  <m:mo stretchy="false">(</m:mo><m:mn>–1</m:mn><m:mo>,</m:mo><m:mn>1</m:mn><m:mo stretchy="false">)</m:mo>
 </m:mrow>
</m:math> </para></problem>
<solution id="fs-id1165134350004"><para id="fs-id1165134350005">Sí</para></solution></exercise>
<exercise id="fs-id1165134350008"><problem id="fs-id1165134350009"><para id="fs-id1165134350010"> <m:math>
 <m:mtable>
  <m:mtr>
   <m:mtd>
    <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>8</m:mn><m:mi>y</m:mi><m:mo>=</m:mo><m:mn>43</m:mn><m:mspace width="0.5em"/>
   </m:mtd>
  </m:mtr>
  <m:mtr>
   <m:mtd>
    <m:mn>3</m:mn><m:mi>x</m:mi><m:mn>−2</m:mn><m:mi>y</m:mi><m:mo>=</m:mo><m:mn>–1</m:mn>
   </m:mtd>
  </m:mtr>
 </m:mtable>

</m:math> y <m:math>
 <m:mrow>

<para id="fs-id1165137844204"> <m:math>
<m:mrow>
<m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mo>−</m:mo><m:mn>58</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>17,3</m:mn>

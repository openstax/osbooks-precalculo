          <m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>b</m:mi><m:mo>=</m:mo><m:mo>-</m:mo><m:mfrac>
            <m:mn>1</m:mn>
            <m:mn>4</m:mn>
          </m:mfrac>
          <m:mo>,</m:mo>
        </m:mrow>
      </m:math> con la <m:math>
        <m:mrow>
          <m:mi>a</m:mi>
        </m:mrow>
      </m:math> y <m:math>
        <m:mrow>
          <m:mi>b</m:mi>
        </m:mrow>
      </m:math> ambos en el intervalo <m:math>
        <m:mrow>
          <m:mrow><m:mo>[</m:mo> <m:mrow>
            <m:mfrac>
              <m:mi>π</m:mi>
              <m:mn>2</m:mn>
            </m:mfrac>
            <m:mo>,</m:mo><m:mi>π</m:mi>
          </m:mrow> <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo>
        </m:mrow>
      </m:math> calcule <m:math>
        <m:mrow>
          <m:mi>sen</m:mi><m:mo stretchy="false">(</m:mo><m:mi>a</m:mi><m:mo>+</m:mo><m:mi>b</m:mi><m:mo stretchy="false">)</m:mo>
        </m:mrow>
      </m:math> y <m:math>
        <m:mrow>
          <m:mi>cos</m:mi><m:mo stretchy="false">(</m:mo><m:mi>a</m:mi><m:mo>-</m:mo><m:mi>b</m:mi><m:mo stretchy="false">)</m:mo><m:mo>.</m:mo>
        </m:mrow>
      </m:math> </para></problem></exercise>
      <exercise id="fs-id1256165" class="material-set-2"><problem id="fs-id1256166"><para id="fs-id1256167">Dado que <m:math>
        <m:mrow>
          <m:mi>sen</m:mi><m:mspace width="0.3em"/><m:mi>a</m:mi><m:mo>=</m:mo><m:mfrac>
            <m:mn>4</m:mn>
            <m:mn>5</m:mn>
          </m:mfrac>
          <m:mo>,</m:mo>
        </m:mrow>
      </m:math> y <m:math>
        <m:mrow>
          <m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>b</m:mi><m:mo>=</m:mo><m:mfrac>
            <m:mn>1</m:mn>
            <m:mn>3</m:mn>
          </m:mfrac>
          <m:mo>,</m:mo>
        </m:mrow>
      </m:math> con la <m:math>
        <m:mrow>
          <m:mi>a</m:mi>
        </m:mrow>
      </m:math> y <m:math>
        <m:mrow>
          <m:mi>b</m:mi>
        </m:mrow>
      </m:math> ambos en el intervalo <m:math>
        <m:mrow>
          <m:mrow><m:mo>[</m:mo> <m:mrow>
            <m:mn>0</m:mn><m:mo>,</m:mo><m:mfrac>
              <m:mi>π</m:mi>
              <m:mn>2</m:mn>
            </m:mfrac>

          </m:mrow> <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo>
        </m:mrow>
      </m:math> calcule <m:math>
        <m:mrow>
          <m:mi>sen</m:mi><m:mo stretchy="false">(</m:mo><m:mi>a</m:mi><m:mo>-</m:mo><m:mi>b</m:mi><m:mo stretchy="false">)</m:mo>
        </m:mrow>
      </m:math> y <m:math>
        <m:mrow>
          <m:mi>cos</m:mi><m:mo stretchy="false">(</m:mo><m:mi>a</m:mi><m:mo>+</m:mo><m:mi>b</m:mi><m:mo stretchy="false">)</m:mo><m:mo>.</m:mo>
        </m:mrow>
      </m:math>
      </para></problem>
      <solution id="fs-id2011927">
        <para id="fs-id2011929"> <m:math><m:mrow>         <m:mi>sen</m:mi><m:mo stretchy="false">(</m:mo><m:mi>a</m:mi><m:mo>-</m:mo><m:mi>b</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mrow><m:mo>(</m:mo>
                  <m:mrow>
                    <m:mfrac>
                      <m:mn>4</m:mn>
                      <m:mn>5</m:mn>
                    </m:mfrac>

                  </m:mrow>
                  <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
                    <m:mrow>
                      <m:mfrac>
                        <m:mn>1</m:mn>
                        <m:mn>3</m:mn>
                      </m:mfrac>

                    </m:mrow>
                    <m:mo>)</m:mo></m:mrow><m:mo>-</m:mo><m:mrow><m:mo>(</m:mo>
                      <m:mrow>
                        <m:mfrac>
                          <m:mn>3</m:mn>
                          <m:mn>5</m:mn>
                        </m:mfrac>

                      </m:mrow>
                      <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
                        <m:mrow>
                          <m:mfrac>
                            <m:mrow>
                              <m:mn>2</m:mn><m:msqrt>
                                <m:mn>2</m:mn>
                              </m:msqrt>

                            </m:mrow>
                            <m:mn>3</m:mn>
                          </m:mfrac>

                        </m:mrow>
                        <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mfrac>
                          <m:mrow>
                            <m:mn>4</m:mn><m:mo>- −</m:mo><m:mn>6</m:mn><m:msqrt>
                              <m:mn>2</m:mn>
                            </m:msqrt>

                          </m:mrow>
                          <m:mrow>
                            <m:mn>15</m:mn>
                          </m:mrow>
                        </m:mfrac>
</m:mrow></m:math> <newline/> <m:math><m:mrow>
                <m:mi>cos</m:mi><m:mo stretchy="false">(</m:mo><m:mi>a</m:mi><m:mo>+</m:mo><m:mi>b</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mrow><m:mo>(</m:mo>
                  <m:mrow>
                    <m:mfrac>
                      <m:mn>3</m:mn>
                      <m:mn>5</m:mn>
                    </m:mfrac>

                  </m:mrow>
                  <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
                    <m:mrow>
                      <m:mfrac>
                        <m:mn>1</m:mn>
                        <m:mn>3</m:mn>
                      </m:mfrac>

                    </m:mrow>
                    <m:mo>)</m:mo></m:mrow><m:mo>-</m:mo><m:mrow><m:mo>(</m:mo>
                      <m:mrow>
                        <m:mfrac>
                          <m:mn>4</m:mn>
                          <m:mn>5</m:mn>
                        </m:mfrac>

                      </m:mrow>
                      <m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
                        <m:mrow>
                          <m:mfrac>
                            <m:mrow>
                              <m:mn>2</m:mn><m:msqrt>
                                <m:mn>2</m:mn>
                              </m:msqrt>

                            </m:mrow>
                            <m:mn>3</m:mn>
                          </m:mfrac>

                        </m:mrow>
                        <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mfrac>
                          <m:mrow>
                            <m:mn>3</m:mn><m:mo>-</m:mo><m:mn>8</m:mn><m:msqrt>
                              <m:mn>2</m:mn>
                            </m:msqrt>

                          </m:mrow>
                          <m:mrow>
                            <m:mn>15</m:mn>
                          </m:mrow>
                        </m:mfrac></m:mrow>
        </m:math>
        </para></solution></exercise>

      <para id="fs-id2478807">En los siguientes ejercicios, halle el valor exacto de cada expresión.</para>
      <exercise id="fs-id2478810" class="material-set-2"><problem id="fs-id2478811"><para id="fs-id2478812"> <m:math>
        <m:mrow>
          <m:mi>sen</m:mi><m:mrow><m:mo>(</m:mo>
            <m:mrow>
              <m:msup>
                <m:mrow>
                  <m:mi>cos</m:mi>
                </m:mrow>
                <m:mrow>
                  <m:mo>- −</m:mo><m:mn>1</m:mn>
                </m:mrow>
              </m:msup>
              <m:mo stretchy="false">(</m:mo><m:mn>0</m:mn><m:mo stretchy="false">)</m:mo><m:mo>-</m:mo><m:msup>
                <m:mrow>
                  <m:mi>cos</m:mi>
                </m:mrow>
                <m:mrow>
                  <m:mo>- −</m:mo><m:mn>1</m:mn>
                </m:mrow>
              </m:msup>
              <m:mrow><m:mo>(</m:mo>
                <m:mrow>
                  <m:mfrac>
                    <m:mn>1</m:mn>
                    <m:mn>2 </m:mn>
                  </m:mfrac>

                </m:mrow>
                <m:mo>)</m:mo></m:mrow>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow>
        </m:mrow>
      </m:math> grandes.
      </para></problem></exercise>
      <exercise id="fs-id2126676" class="material-set-2"><problem id="fs-id2126677"><para id="fs-id2126678"> <m:math>
        <m:mrow>
          <m:mi>cos</m:mi><m:mrow><m:mo>(</m:mo>
            <m:mrow>
              <m:msup>
                <m:mrow>
                  <m:mi>cos</m:mi>
                </m:mrow>
                <m:mrow>
                  <m:mo>- −</m:mo><m:mn>1</m:mn>
                </m:mrow>
              </m:msup>
              <m:mrow><m:mo>(</m:mo>
                <m:mrow>
                  <m:mfrac>
                    <m:mrow>
                      <m:msqrt>
                        <m:mn>2</m:mn>
                      </m:msqrt>

                    </m:mrow>
                    <m:mn>2</m:mn>
                  </m:mfrac>

                </m:mrow>
                <m:mo>)</m:mo></m:mrow><m:mo>+</m:mo><m:msup>
                  <m:mrow>
                    <m:mi>sen</m:mi>
                  </m:mrow>
                  <m:mrow>
                    <m:mo>- −</m:mo><m:mn>1</m:mn>
                  </m:mrow>
                </m:msup>
              <m:mrow><m:mo>(</m:mo>
                <m:mrow>
                  <m:mfrac>
                    <m:mrow>
                      <m:msqrt>
                        <m:mn>3</m:mn>
                      </m:msqrt>

                    </m:mrow>
                    <m:mn>2 </m:mn>
                  </m:mfrac>

                </m:mrow>
                <m:mo>)</m:mo></m:mrow>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow>
        </m:mrow>
      </m:math> grandes.
      </para></problem>
        <solution id="fs-id2229929">
          <para id="fs-id2229931"> <m:math>
            <m:mrow>
              <m:mfrac>
                <m:mrow>
                  <m:msqrt>
                    <m:mn>2 </m:mn>
                  </m:msqrt>
                  <m:mo>-</m:mo><m:msqrt>
                    <m:mn>6</m:mn>
                  </m:msqrt>

                </m:mrow>
                <m:mn>4</m:mn>
              </m:mfrac>

            </m:mrow>
          </m:math>
          </para>
        </solution></exercise>
      <exercise id="fs-id2337567" class="material-set-2"><problem id="fs-id2336035"><para id="fs-id2336036"> <m:math>

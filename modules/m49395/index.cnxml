        </m:mfrac>

       </m:mrow>
      <m:mo>)</m:mo></m:mrow>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mtext>                       </m:mtext><m:mo>=</m:mo><m:mfrac>
       <m:mrow>
        <m:msqrt>
         <m:mn>6</m:mn>
        </m:msqrt>

       </m:mrow>
       <m:mn>4</m:mn>
      </m:mfrac>
      <m:mo>–</m:mo><m:mfrac>

      <m:mi>sen</m:mi><m:mo stretchy="false">(</m:mo><m:msup>
       <m:mrow>
        <m:mn>135</m:mn>
       </m:mrow>
       <m:mo>∘</m:mo>
      </m:msup>
      <m:mo>- −</m:mo><m:msup>
       <m:mrow>
        <m:mn>120</m:mn>

      <exercise id="fs-id1831266" class="material-set-2"><problem id="fs-id1831267"><para id="fs-id1831268"> <m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:mi>cos</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo>+</m:mo><m:mi>h</m:mi><m:mo stretchy="false">)</m:mo><m:mo>-</m:mo><m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi>
   </m:mrow>
   <m:mi>h</m:mi>
  </m:mfrac>
  <m:mo>=</m:mo><m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi><m:mfrac>
   <m:mrow>
    <m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>h</m:mi><m:mo>-</m:mo><m:mn>1</m:mn>
   </m:mrow>
   <m:mi>h</m:mi>
  </m:mfrac>
  <m:mo>- −</m:mo><m:mi>sen</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi><m:mfrac>
   <m:mrow>
    <m:mi>sen</m:mi><m:mspace width="0.3em"/><m:mi>h</m:mi>
   </m:mrow>
   <m:mi>h</m:mi>
  </m:mfrac>

 </m:mrow>
</m:math>
</para></problem>
        <solution id="fs-id2167167">
          <para id="fs-id2167170"> <m:math>
            <m:mtable>
              <m:mtr>
                <m:mtd>
                  <m:mfrac>
                    <m:mrow>
                      <m:mi>cos</m:mi><m:mrow><m:mo>(</m:mo>
                        <m:mrow>
                          <m:mi>x</m:mi><m:mo>+</m:mo><m:mi>h</m:mi>
                        </m:mrow>
                        <m:mo>)</m:mo></m:mrow><m:mo>-</m:mo><m:mi>cos</m:mi><m:mi>x</m:mi>
                    </m:mrow>
                    <m:mi>h</m:mi>
                  </m:mfrac>
                  <m:mo>=</m:mo>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd>
                  <m:mfrac>
                    <m:mrow>
                      <m:mi>cos</m:mi><m:mi>x</m:mi><m:mi>cosh</m:mi><m:mo>- −</m:mo><m:mi>sen</m:mi><m:mi>x</m:mi><m:mi>senoh</m:mi><m:mo>- −</m:mo><m:mi>cos</m:mi><m:mi>x</m:mi>
                    </m:mrow>
                    <m:mi>h</m:mi>
                  </m:mfrac>
                  <m:mo>=</m:mo>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd>
                  <m:mfrac>
                    <m:mrow>
                      <m:mi>cos</m:mi><m:mi>x</m:mi><m:mo stretchy="false">(</m:mo><m:mi>cosh</m:mi><m:mo>- −</m:mo><m:mn>1</m:mn><m:mo stretchy="false">)</m:mo><m:mo>-</m:mo><m:mi>sen</m:mi><m:mi>x</m:mi><m:mi>senoh</m:mi>
                    </m:mrow>
                    <m:mi>h</m:mi>
                  </m:mfrac>
                  <m:mo>=</m:mo><m:mi>cos</m:mi><m:mi>x</m:mi><m:mfrac>
                    <m:mrow>
                      <m:mi>cos</m:mi><m:mi>h</m:mi><m:mo>-</m:mo><m:mn>1</m:mn>
                    </m:mrow>
                    <m:mi>h</m:mi>
                  </m:mfrac>
                  <m:mo>- −</m:mo><m:mi>sen</m:mi><m:mi>x</m:mi><m:mfrac>
                    <m:mrow>
                      <m:mi>sen</m:mi><m:mi>h</m:mi>
                    </m:mrow>
                    <m:mi>h</m:mi>
                  </m:mfrac>

                </m:mtd>
              </m:mtr>
            </m:mtable>

          </m:math>
          </para>
        </solution></exercise>



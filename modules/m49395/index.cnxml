      <exercise id="fs-id2024685" class="material-set-2"><problem id="fs-id2024686"><para id="fs-id2024687"> <m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:mi>cos</m:mi><m:mo stretchy="false">(</m:mo><m:mi>a</m:mi><m:mo>+</m:mo><m:mi>b</m:mi><m:mo stretchy="false">)</m:mo>
   </m:mrow>
   <m:mrow>
    <m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>a</m:mi><m:mspace width="0.3em"/><m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>b</m:mi>
   </m:mrow>
  </m:mfrac>
  <m:mo>=</m:mo><m:mn>1</m:mn><m:mo>-</m:mo><m:mi>tan</m:mi><m:mspace width="0.3em"/><m:mi>a</m:mi><m:mspace width="0.3em"/><m:mi>tan</m:mi><m:mspace width="0.3em"/><m:mi>b</m:mi>
 </m:mrow>
</m:math>
</para></problem>
        <solution id="fs-id2810957">
          <para id="fs-id2810960"> <m:math>
            <m:mtable>
              <m:mtr>
                <m:mtd>
                  <m:mfrac>
                    <m:mrow>
                      <m:mi>cos</m:mi><m:mrow><m:mo>(</m:mo>
                        <m:mrow>
                          <m:mi>a</m:mi><m:mo>+</m:mo><m:mi>b</m:mi>
                        </m:mrow>
                        <m:mo>)</m:mo></m:mrow>
                    </m:mrow>
                    <m:mrow>
                      <m:mi>cos</m:mi><m:mi>a</m:mi><m:mi>cos</m:mi><m:mi>b</m:mi>
                    </m:mrow>
                  </m:mfrac>
                  <m:mo>=</m:mo>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd>
                  <m:mfrac>
                    <m:mrow>
                      <m:mi>cos</m:mi><m:mi>a</m:mi><m:mi>cos</m:mi><m:mi>b</m:mi>
                    </m:mrow>
                    <m:mrow>
                      <m:mi>cos</m:mi><m:mi>a</m:mi><m:mi>cos</m:mi><m:mi>b</m:mi>
                    </m:mrow>
                  </m:mfrac>
                  <m:mo>- −</m:mo><m:mfrac>
                    <m:mrow>
                      <m:mi>sen</m:mi><m:mi>a</m:mi><m:mi>sen</m:mi><m:mi>b</m:mi>
                    </m:mrow>
                    <m:mrow>
                      <m:mi>cos</m:mi><m:mi>a</m:mi><m:mi>cos</m:mi><m:mi>b</m:mi>
                    </m:mrow>
                  </m:mfrac>
                  <m:mo>=</m:mo><m:mn>1</m:mn><m:mo>-</m:mo><m:mi>tan</m:mi><m:mi>a</m:mi><m:mi>tan</m:mi><m:mi>b</m:mi>
                </m:mtd>
              </m:mtr>
            </m:mtable>

          </m:math>
          </para>
        </solution></exercise>
      <exercise id="fs-id2467728" class="material-set-2"><problem id="fs-id2467729"><para id="fs-id2467730"> <m:math>

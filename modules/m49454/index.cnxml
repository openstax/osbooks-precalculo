<exercise id="fs-id1165135698716" class="material-set-2">
<problem id="fs-id1165135698718">
<para id="fs-id1165135698720"> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mi>x</m:mi>
  <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mrow><m:mo>{</m:mo> <m:mrow>

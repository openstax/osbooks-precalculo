<exercise id="fs-id1165135683329" class="material-set-2">
<problem id="fs-id1165135683331">
<para id="fs-id1165135683333"> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mi>x</m:mi>
  <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mrow><m:mo>{</m:mo> <m:mrow>
   <m:mtable columnalign="left">
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mfrac>
        <m:mrow>
         <m:msup>
          <m:mi>x</m:mi>
          <m:mn>2 </m:mn>
         </m:msup>
         <m:mo>-</m:mo><m:mn>9</m:mn>
        </m:mrow>
        <m:mrow>
         <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>3</m:mn>
        </m:mrow>
       </m:mfrac>
       <m:mo>,</m:mo>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>x</m:mi><m:mo>&lt;</m:mo><m:mo>-</m:mo><m:mn>3</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>x</m:mi><m:mo>-</m:mo><m:mn>9</m:mn><m:mo>,</m:mo>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>x</m:mi><m:mo>=</m:mo><m:mo>-</m:mo><m:mn>3</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mfrac>
        <m:mn>1</m:mn>
        <m:mi>x</m:mi>
       </m:mfrac>
       <m:mo>,</m:mo>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>x</m:mi><m:mo>></m:mo><m:mo>-</m:mo><m:mn>3</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>

   </m:mtable><m:mtext>    </m:mtext><m:mi>a</m:mi><m:mo>=</m:mo><m:mo>-</m:mo><m:mn>3</m:mn>
  </m:mrow> </m:mrow>
 </m:mrow>
</m:math> </para></problem>
<solution id="fs-id1165135683485">
<para id="fs-id1165135683487"> <m:math>
 <m:mrow>
  <m:munder>
   <m:mrow>
    <m:mi>lím</m:mi>
   </m:mrow>
   <m:mrow>
    <m:mi>x</m:mi><m:mrow><m:mo stretchy="false">→</m:mo></m:mrow><m:mo>-</m:mo><m:msup>
     <m:mn>3</m:mn>
     <m:mo>-</m:mo>
    </m:msup>

   </m:mrow>
  </m:munder>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mo>-</m:mo><m:mn>6</m:mn>
 </m:mrow>
</m:math>, <m:math>
 <m:mrow>
  <m:munder>
   <m:mrow>
    <m:mi>lím</m:mi>
   </m:mrow>
   <m:mrow>
    <m:mi>x</m:mi><m:mrow><m:mo stretchy="false">→</m:mo></m:mrow><m:mo>-</m:mo><m:msup>
     <m:mn>3</m:mn>
     <m:mo>+</m:mo>
    </m:msup>

   </m:mrow>
  </m:munder>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mo>-</m:mo><m:mfrac>
   <m:mn>1</m:mn>
   <m:mn>3</m:mn>
  </m:mfrac>

 </m:mrow>
</m:math> </para>
<para id="fs-id1165135683605">Por lo tanto, <m:math>
 <m:mrow>
  <m:munder>
   <m:mrow>
    <m:mi>lím</m:mi>
   </m:mrow>
   <m:mrow>
    <m:mi>x</m:mi><m:mrow><m:mo stretchy="false">→</m:mo></m:mrow><m:mo>-</m:mo><m:mn>3</m:mn>
   </m:mrow>
  </m:munder>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo>
 </m:mrow>
</m:math> no existe.</para>
</solution>
</exercise>


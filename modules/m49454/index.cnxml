  </m:msup>

 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id1165135693080">
<para id="fs-id1165135693082">Continua en <m:math>
 <m:mrow>
  <m:mo stretchy="false">(</m:mo><m:mn>0</m:mn><m:mo>,</m:mo><m:mi>∞</m:mi><m:mo stretchy="false">)</m:mo>
 </m:mrow>
</m:math> grandes. </para>
</solution>
</exercise><exercise id="fs-id1165135693110">
<problem id="fs-id1165135693113">
<para id="fs-id1165135693115"> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mi>x</m:mi>
  <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:msup>
   <m:mi>e</m:mi>
   <m:mrow>
    <m:mn>2</m:mn><m:mi>x</m:mi>
   </m:mrow>
  </m:msup>

 </m:mrow>
</m:math> </para>
</problem>
</exercise>

<exercise id="fs-id1165135693153">
<problem id="fs-id1165135693155">
<para id="fs-id1165135693157"> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:msqrt>
   <m:mrow>
    <m:mi>x</m:mi><m:mo>-</m:mo><m:mn>4</m:mn>

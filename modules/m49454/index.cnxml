        <m:mn>1</m:mn>
        <m:mrow>
         <m:mn>2 </m:mn><m:mo>-</m:mo><m:mi>x</m:mi>
        </m:mrow>
       </m:mfrac>
       <m:mo>,</m:mo>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>x</m:mi><m:mo>≠</m:mo><m:mn>2 </m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="left">
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mn>3</m:mn><m:mo>,</m:mo>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="left">
      <m:mrow>
       <m:mi>x</m:mi><m:mo>=</m:mo><m:mn>2 </m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>

   </m:mtable>
  </m:mrow> </m:mrow><m:mspace width="0.5em"/><m:mi>a</m:mi><m:mo>=</m:mo><m:mn>2 </m:mn>
 </m:mrow>
</m:math> </para></problem>
<solution id="fs-id1165135690120">
<para id="fs-id1165135690122"> <m:math>
 <m:mrow>
  <m:munder>

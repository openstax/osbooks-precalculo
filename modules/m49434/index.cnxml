     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="right">
    <m:mtd columnalign="right">
     <m:mrow>
      <m:mi>y</m:mi><m:mn>−2</m:mn><m:mi>c</m:mi><m:mo>=</m:mo><m:mn>0</m:mn>
     </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable>
 </m:mrow>
</m:math>
</equation></problem>
<solution id="fs-id1165134547437">
<para id="fs-id1165135567424">Escriba la matriz aumentada.</para>
  <equation id="fs-id1165135567428" class="unnumbered"><label/> <m:math display="block">
 <m:mrow>
  <m:mrow><m:mo>[</m:mo> <m:mrow>
   <m:mtable columnalign="right">
    <m:mtr columnalign="right">
     <m:mtd columnalign="right">
      <m:mrow>
<m:mn>−1</m:mn>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mrow>
<m:mn>−2</m:mn>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mn>1</m:mn>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="right">
     <m:mtd columnalign="right">
      <m:mn>2 </m:mn>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mn>3</m:mn>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mn>0</m:mn>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="right">
     <m:mtd columnalign="right">
      <m:mn>0</m:mn>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mn>1</m:mn>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mrow>
<m:mn>−2</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>

   </m:mtable><m:mspace width="0.5em"/><m:mtext> </m:mtext><m:mrow><m:mo>|</m:mo><m:mrow>
    <m:mspace width="0.5em"/><m:mtext> </m:mtext><m:mtable columnalign="right">
     <m:mtr columnalign="right">
      <m:mtd columnalign="right">
       <m:mrow>
        <m:mn>−1</m:mn>
       </m:mrow>
      </m:mtd>
     </m:mtr>
     <m:mtr columnalign="right">
      <m:mtd columnalign="right">
       <m:mn>2</m:mn>
      </m:mtd>

       <m:mn>2</m:mn>
      </m:mfrac>
      <m:mo>+</m:mo><m:mfrac>
       <m:mrow>
        <m:mi>y</m:mi><m:mo>+</m:mo><m:mn>5</m:mn>
       </m:mrow>
       <m:mn>2</m:mn>
      </m:mfrac>
      <m:mo>+</m:mo><m:mfrac>
       <m:mrow>
        <m:mi>z</m:mi><m:mo>+</m:mo><m:mn>5</m:mn>
       </m:mrow>
       <m:mn>2 </m:mn>
      </m:mfrac>
      <m:mo>=</m:mo><m:mn>8</m:mn>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow><m:mi>x</m:mi><m:mo>+</m:mo><m:mi>y</m:mi><m:mo>+</m:mo><m:mi>z</m:mi><m:mo>=</m:mo><m:mn>1</m:mn>
     </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable>
 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id1165135263629"><para id="fs-id1165133045223"> <m:math>
 <m:mrow>
  <m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mi>x</m:mi><m:mo>,</m:mo><m:mfrac>
     <m:mrow>
      <m:mn>31</m:mn>
     </m:mrow>
     <m:mrow>
      <m:mn>28</m:mn>
     </m:mrow>
    </m:mfrac>
    <m:mo>-</m:mo><m:mfrac>
     <m:mrow>
      <m:mn>3</m:mn><m:mi>x</m:mi>
     </m:mrow>
     <m:mn>4</m:mn>
    </m:mfrac>
    <m:mo>,</m:mo><m:mfrac>
     <m:mn>1</m:mn>
     <m:mrow>
      <m:mn>28</m:mn>
     </m:mrow>
    </m:mfrac>
    <m:mo stretchy="false">(</m:mo><m:mn>−7</m:mn><m:mi>x</m:mi><m:mn>−3</m:mn><m:mo stretchy="false">)</m:mo>
   </m:mrow>
  <m:mo>)</m:mo></m:mrow>
 </m:mrow>
</m:math> grandes. </para></solution>
</exercise>

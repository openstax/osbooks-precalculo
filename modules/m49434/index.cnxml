     </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable>
 </m:mrow>
</m:math>
</equation></problem>
<solution id="fs-id1165134189887"><para id="fs-id1165137834565"><m:math>
 <m:mrow>
  <m:mrow><m:mo>[</m:mo> <m:mrow>
   <m:mrow><m:mrow>
    <m:mtable>
     <m:mtr>
      <m:mtd>
       <m:mrow>
        <m:mn>1</m:mn>
       </m:mrow>
      </m:mtd>
      <m:mtd>
       <m:mrow>
        <m:mo>-</m:mo><m:mfrac>
         <m:mn>5</m:mn>
         <m:mn>2 </m:mn>

    <m:mtd columnalign="right">
     <m:mrow>
      <m:mo>-</m:mo><m:mi>x</m:mi><m:mn>−2</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mi>z</m:mi><m:mo>=</m:mo><m:mn>−1</m:mn>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="right">
    <m:mtd columnalign="right">
     <m:mrow>

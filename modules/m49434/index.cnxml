          <m:mtd columnalign="right">
           <m:mrow>
            <m:mfrac>
             <m:mn>4</m:mn>
             <m:mn>3</m:mn>
            </m:mfrac>

           </m:mrow>
          </m:mtd>
          <m:mtd columnalign="right">
           <m:mrow/>
          </m:mtd>
         </m:mtr>
         <m:mtr columnalign="right">
          <m:mtd columnalign="right">
           <m:mn>0</m:mn>
          </m:mtd>
          <m:mtd columnalign="right">

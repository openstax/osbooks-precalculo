      <m:mo>-</m:mo><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>3</m:mn><m:mi>y</m:mi><m:mo>=</m:mo><m:mo>–</m:mo><m:mn>4</m:mn>
     </m:mrow>
    </m:mtd>

<exercise id="fs-id1165135363159"><problem id="fs-id1165135363160"><para id="fs-id1165135363161"> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:msup>

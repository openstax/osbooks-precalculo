<exercise id="fs-id1165134060477"><problem id="fs-id1165134060478"><para id="fs-id1165134060479"> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:msup>

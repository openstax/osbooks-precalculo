<exercise id="fs-id1165133289649" class="material-set-2"><problem id="fs-id1165133289650"><para id="fs-id1165133289651"> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:msup>

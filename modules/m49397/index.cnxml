      <exercise id="fs-id535203" class="material-set-2"><problem id="fs-id535204"><para id="fs-id2872155"> <m:math>
        <m:mrow>
          <m:mn>4</m:mn><m:mspace width="0.5em"/><m:mi>sen</m:mi><m:mrow><m:mo>(</m:mo>
            <m:mrow>
              <m:mn>3</m:mn><m:mi>x</m:mi>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow><m:mi>cos</m:mi><m:mrow><m:mo>(</m:mo>
              <m:mrow>
                <m:mn>4</m:mn><m:mi>x</m:mi>
              </m:mrow>
              <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>2</m:mn><m:mspace width="0.5em"/><m:mi>sen</m:mi><m:mrow><m:mo>(</m:mo>
                <m:mrow>
                  <m:mn>7</m:mn><m:mi>x</m:mi>
                </m:mrow>
                <m:mo>)</m:mo></m:mrow><m:mo>-</m:mo><m:mn>2</m:mn><m:mspace width="0.5em"/><m:mi>sen</m:mi><m:mi>x</m:mi>
        </m:mrow>
      </m:math>
      </para></problem>
        <solution id="fs-id2589385">
          <para id="fs-id1203086"> <m:math display="block">
 <m:mrow>
  <m:mtable columnalign="left">
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mn>2</m:mn><m:mspace width="0.5em"/><m:mi>sen</m:mi><m:mo stretchy="false">(</m:mo><m:mn>7</m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>-</m:mo><m:mn>2</m:mn><m:mspace width="0.5em"/><m:mi>sen</m:mi><m:mi>x</m:mi><m:mo>=</m:mo><m:mn>2</m:mn><m:mspace width="0.5em"/><m:mi>sen</m:mi><m:mo stretchy="false">(</m:mo><m:mn>4</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>3</m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>-</m:mo><m:mn>2</m:mn><m:mspace width="0.5em"/><m:mi>sen</m:mi><m:mo stretchy="false">(</m:mo><m:mn>4</m:mn><m:mi>x</m:mi><m:mo>-</m:mo><m:mn>3</m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mn>2 </m:mn><m:mo stretchy="false">(</m:mo><m:mi>sen</m:mi><m:mo stretchy="false">(</m:mo><m:mn>4</m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mi>cos</m:mi><m:mo stretchy="false">(</m:mo><m:mn>3</m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>+</m:mo><m:mi>sen</m:mi><m:mo stretchy="false">(</m:mo><m:mn>3</m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mi>cos</m:mi><m:mo stretchy="false">(</m:mo><m:mn>4</m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo stretchy="false">)</m:mo><m:mo>-</m:mo><m:mn>2 </m:mn><m:mo stretchy="false">(</m:mo><m:mi>sen</m:mi><m:mo stretchy="false">(</m:mo><m:mn>4</m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mi>cos</m:mi><m:mo stretchy="false">(</m:mo><m:mn>3</m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>-</m:mo><m:mi>sen</m:mi><m:mo stretchy="false">(</m:mo><m:mn>3</m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mi>cos</m:mi><m:mo stretchy="false">(</m:mo><m:mn>4</m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mn>2</m:mn><m:mspace width="0.5em"/><m:mi>sen</m:mi><m:mo stretchy="false">(</m:mo><m:mn>4</m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mi>cos</m:mi><m:mo stretchy="false">(</m:mo><m:mn>3</m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>+</m:mo><m:mn>2</m:mn><m:mspace width="0.5em"/><m:mi>sen</m:mi><m:mo stretchy="false">(</m:mo><m:mn>3</m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mi>cos</m:mi><m:mo stretchy="false">(</m:mo><m:mn>4</m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo stretchy="false">)</m:mo><m:mo>-</m:mo><m:mn>2</m:mn><m:mspace width="0.5em"/><m:mi>sen</m:mi><m:mo stretchy="false">(</m:mo><m:mn>4</m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mi>cos</m:mi><m:mo stretchy="false">(</m:mo><m:mn>3</m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>+</m:mo><m:mn>2</m:mn><m:mspace width="0.5em"/><m:mi>sen</m:mi><m:mo stretchy="false">(</m:mo><m:mn>3</m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mi>cos</m:mi><m:mo stretchy="false">(</m:mo><m:mn>4</m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mn>4</m:mn><m:mspace width="0.5em"/><m:mi>sen</m:mi><m:mo stretchy="false">(</m:mo><m:mn>3</m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mi>cos</m:mi><m:mo stretchy="false">(</m:mo><m:mn>4</m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow/>
    </m:mtd>
   </m:mtr>

  </m:mtable>
 </m:mrow>
</m:math> grandes.
</para></solution></exercise>
      <exercise id="fs-id2272110" class="material-set-2"><problem id="fs-id2272111"><para id="fs-id2272112"> <m:math>
        <m:mrow>
          <m:mfrac>
            <m:mrow>
              <m:mn>6</m:mn><m:mspace width="0.5em"/><m:mi>cos</m:mi><m:mrow><m:mo>(</m:mo>
                <m:mrow>
                  <m:mn>8</m:mn><m:mi>x</m:mi>
                </m:mrow>
                <m:mo>)</m:mo></m:mrow><m:mi>sen</m:mi><m:mrow><m:mo>(</m:mo>
                  <m:mrow>
                    <m:mn>2 </m:mn><m:mi>x</m:mi>
                  </m:mrow>
                  <m:mo>)</m:mo></m:mrow>
            </m:mrow>
            <m:mrow>
              <m:mi>sen</m:mi><m:mrow><m:mo>(</m:mo>
                <m:mrow>
                  <m:mo>- −</m:mo><m:mn>6</m:mn><m:mi>x</m:mi>
                </m:mrow>
                <m:mo>)</m:mo></m:mrow>
            </m:mrow>
          </m:mfrac>
          <m:mo>=</m:mo><m:mn>−3</m:mn><m:mspace width="0.5em"/><m:mi>sen</m:mi><m:mrow><m:mo>(</m:mo>
            <m:mrow>
              <m:mn>10</m:mn><m:mi>x</m:mi>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow><m:mi>csc</m:mi><m:mrow><m:mo>(</m:mo>
              <m:mrow>
                <m:mn>6</m:mn><m:mi>x</m:mi>

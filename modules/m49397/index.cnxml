      <exercise id="fs-id1316701"><problem id="fs-id1316702"><para id="fs-id1316703"> <m:math>
        <m:mrow>
          <m:mi>sen</m:mi><m:mo stretchy="false">(</m:mo><m:mi>- −</m:mi><m:msup>
            <m:mrow>

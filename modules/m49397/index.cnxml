        </m:mrow>
      </m:math>
      </para></problem></exercise>
      <exercise id="fs-id1855718" class="material-set-2"><problem id="fs-id1855719"><para id="fs-id1855720"> <m:math>
        <m:mrow>
          <m:mi>sen</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi><m:mo>+</m:mo><m:mi>sen</m:mi><m:mrow><m:mo>(</m:mo>
            <m:mrow>
              <m:mn>3</m:mn><m:mi>x</m:mi>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>4</m:mn><m:mspace width="0.5em"/><m:mi>sen</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi><m:mspace width="0.5em"/><m:msup>
              <m:mrow>
                <m:mi>cos</m:mi>
              </m:mrow>
              <m:mn>2</m:mn>
            </m:msup>
          <m:mi>x</m:mi>
        </m:mrow>
      </m:math>
      </para></problem>
        <solution id="fs-id2564768">
          <para id="fs-id2564770"> <m:math><m:mrow>
                  <m:mi>sen</m:mi><m:mspace width="0.2em"/><m:mi>x</m:mi><m:mo>+</m:mo><m:mi>sen</m:mi><m:mrow><m:mo>(</m:mo>
                    <m:mrow>
                      <m:mn>3</m:mn><m:mi>x</m:mi>
                    </m:mrow>
                    <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>2</m:mn><m:mspace width="0.2em"/><m:mi>sen</m:mi><m:mrow><m:mo>(</m:mo>
                      <m:mrow>
                        <m:mfrac>
                          <m:mrow>
                            <m:mn>4</m:mn><m:mi>x</m:mi>
                          </m:mrow>
                          <m:mn>2 </m:mn>
                        </m:mfrac>

                      </m:mrow>
                      <m:mo>)</m:mo></m:mrow><m:mi>cos</m:mi><m:mrow><m:mo>(</m:mo>
                        <m:mrow>
                          <m:mfrac>
                            <m:mrow>
                              <m:mo>–</m:mo><m:mn>2 </m:mn><m:mi>x</m:mi>
                            </m:mrow>
                            <m:mn>2 </m:mn>
                          </m:mfrac>

                        </m:mrow>
                        <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo></m:mrow></m:math> <newline/> <m:math><m:mrow>
                  <m:mn>2</m:mn><m:mspace width="0.2em"/><m:mi>sen</m:mi><m:mo stretchy="false">(</m:mo><m:mn>2 </m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi><m:mo>=</m:mo><m:mn>2 </m:mn><m:mo stretchy="false">(</m:mo><m:mn>2</m:mn><m:mspace width="0.5em"/><m:mi>sen</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi><m:mspace width="0.3em"/><m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi><m:mo>=</m:mo></m:mrow></m:math> <newline/> <m:math><m:mrow>
                  <m:mn>4</m:mn><m:mspace width="0.5em"/><m:mi>sen</m:mi><m:mspace width="0.2em"/><m:mi>x</m:mi><m:mspace width="0.2em"/><m:msup>
                    <m:mi>cos</m:mi>
                    <m:mn>2</m:mn>
                  </m:msup>
                  <m:mspace width="0.2em"/><m:mi>x</m:mi></m:mrow>
          </m:math>
          </para></solution></exercise>
      <exercise id="fs-id1674000" class="material-set-2"><problem id="fs-id1674001"><para id="fs-id1737682"> <m:math>
        <m:mrow>
          <m:mn>2</m:mn><m:mrow><m:mo>(</m:mo>
            <m:mrow>
              <m:msup>
                <m:mrow>
                  <m:mi>cos</m:mi>
                </m:mrow>
                <m:mn>3</m:mn>
              </m:msup>
              <m:mi>x</m:mi><m:mo>-</m:mo><m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi><m:mspace width="0.5em"/><m:msup>
                <m:mrow>
                  <m:mi>sen</m:mi>
                </m:mrow>
                <m:mn>2</m:mn>
              </m:msup>
              <m:mi>x</m:mi>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mi>cos</m:mi><m:mrow><m:mo>(</m:mo>
              <m:mrow>
                <m:mn>3</m:mn><m:mi>x</m:mi>
              </m:mrow>
              <m:mo>)</m:mo></m:mrow><m:mo>+</m:mo><m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi>
        </m:mrow>
      </m:math>
      </para></problem></exercise>
      <exercise id="fs-id1326275" class="material-set-2"><problem id="fs-id1326276"><para id="fs-id1326277"> <m:math>
        <m:mrow>
          <m:mn>2</m:mn><m:mi>tan</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi><m:mspace width="0.3em"/><m:mi>cos</m:mi><m:mrow><m:mo>(</m:mo>
            <m:mrow>
              <m:mn>3</m:mn><m:mi>x</m:mi>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mi>sec</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi><m:mrow><m:mo>(</m:mo>
              <m:mrow>
                <m:mi>sen</m:mi><m:mrow><m:mo>(</m:mo>
                  <m:mrow>
                    <m:mn>4</m:mn><m:mi>x</m:mi>
                  </m:mrow>
                  <m:mo>)</m:mo></m:mrow><m:mo>-</m:mo><m:mi>sen</m:mi><m:mrow><m:mo>(</m:mo>
                    <m:mrow>
                      <m:mn>2 </m:mn><m:mi>x</m:mi>
                    </m:mrow>
                    <m:mo>)</m:mo></m:mrow>
              </m:mrow>
              <m:mo>)</m:mo></m:mrow>
        </m:mrow>
      </m:math> grandes.
      </para></problem>
        <solution id="fs-id2184022">
          <para id="fs-id2184024"> <m:math><m:mrow>
                  <m:mn>2</m:mn><m:mspace width="0.2em"/><m:mi>tan</m:mi><m:mspace width="0.2em"/><m:mi>x</m:mi><m:mspace width="0.2em"/><m:mi>cos</m:mi><m:mrow><m:mo>(</m:mo>
                    <m:mrow>
                      <m:mn>3</m:mn><m:mi>x</m:mi>
                    </m:mrow>
                    <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mfrac>
                      <m:mrow>
                        <m:mn>2</m:mn><m:mspace width="0.2em"/><m:mi>sen</m:mi><m:mspace width="0.2em"/><m:mi>x</m:mi><m:mspace width="0.2em"/><m:mi>cos</m:mi><m:mo stretchy="false">(</m:mo><m:mn>3</m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>cos</m:mi><m:mspace width="0.2em"/><m:mi>x</m:mi>
                      </m:mrow>
                    </m:mfrac>
                  <m:mo>=</m:mo><m:mfrac>
                    <m:mrow>
                      <m:mn>2 </m:mn><m:mo stretchy="false">(</m:mo><m:mn>0,5</m:mn><m:mo stretchy="false">(</m:mo><m:mi>sen</m:mi><m:mo stretchy="false">(</m:mo><m:mn>4</m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>-</m:mo><m:mi>sen</m:mi><m:mo stretchy="false">(</m:mo><m:mn>2 </m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo stretchy="false">)</m:mo><m:mo stretchy="false">)</m:mo>
                    </m:mrow>
                    <m:mrow>
                      <m:mi>cos</m:mi><m:mspace width="0.2em"/><m:mi>x</m:mi>
                    </m:mrow>
                  </m:mfrac></m:mrow>
</m:math> <newline/> <m:math>
<m:mrow>
                  <m:mo>=</m:mo>
                  <m:mfrac>
                    <m:mn>1</m:mn>
                    <m:mrow>
                      <m:mi>cos</m:mi><m:mspace width="0.2em"/><m:mi>x</m:mi>
                    </m:mrow>
                  </m:mfrac>
                  <m:mrow><m:mo>(</m:mo>
                    <m:mrow>
                      <m:mi>sen</m:mi><m:mo stretchy="false">(</m:mo><m:mn>4</m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>-</m:mo><m:mi>sen</m:mi><m:mo stretchy="false">(</m:mo><m:mn>2 </m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo>
                    </m:mrow>
                    <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mi>sec</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi><m:mrow><m:mo>(</m:mo>
                      <m:mrow>
                        <m:mi>sen</m:mi><m:mrow><m:mo>(</m:mo>
                          <m:mrow>
                            <m:mn>4</m:mn><m:mi>x</m:mi>
                          </m:mrow>
                          <m:mo>)</m:mo></m:mrow><m:mo>-</m:mo><m:mi>sen</m:mi><m:mrow><m:mo>(</m:mo>
                            <m:mrow>
                              <m:mn>2 </m:mn><m:mi>x</m:mi>
                            </m:mrow>
                            <m:mo>)</m:mo></m:mrow>
                      </m:mrow>
                      <m:mo>)</m:mo></m:mrow>
</m:mrow></m:math> grandes.
</para></solution></exercise>
      <exercise id="fs-id2243333"><problem id="fs-id1531317"><para id="fs-id1531318"> <m:math>
        <m:mrow>

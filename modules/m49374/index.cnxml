  <m:mtext>sec</m:mtext><m:mn>40°</m:mn><m:mo>,</m:mo><m:mtext>csc</m:mtext><m:mn>40°</m:mn><m:mo>,</m:mo><m:mtext>tan</m:mtext><m:mn>40°</m:mn><m:mo>,</m:mo>
</m:math> y <m:math>
  <m:mtext>cot</m:mtext><m:mtext>y</m:mtext><m:mn>40°</m:mn><m:mo>.</m:mo>

    </m:math>
    </para></problem>
    <solution id="fs-id1165135196912"><para id="fs-id1165135196914"> <m:math><m:mrow>
        <m:mi>sen</m:mi><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mfrac>
           <m:mrow>
            <m:mo>- −</m:mo><m:mn>7</m:mn><m:mi>π</m:mi></m:mrow>
           <m:mn>4</m:mn>
          </m:mfrac>
         </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mfrac>
          <m:mrow>
           <m:msqrt>
            <m:mn>2 </m:mn>
           </m:msqrt>
          </m:mrow>
          <m:mn>2</m:mn>
         </m:mfrac>
        <m:mo>,</m:mo><m:mi>cos</m:mi><m:mrow><m:mo>(</m:mo>

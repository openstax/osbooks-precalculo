   <m:mn>3</m:mn>
  </m:mfrac>

 </m:mrow>
</m:math>
</para></problem>
    <solution id="fs-id1165134357592">
     <para id="fs-id1165134357594"> <m:math>
      <m:mrow>
       <m:mfrac>
        <m:mrow>
         <m:msqrt>
          <m:mn>3</m:mn>
         </m:msqrt>
        </m:mrow>
        <m:mn>3</m:mn>
       </m:mfrac>
      </m:mrow>
     </m:math>
     </para>
    </solution>
   </exercise>


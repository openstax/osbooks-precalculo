   </m:mrow>
   <m:mn>7</m:mn>
  </m:msub>
  <m:mi>v</m:mi><m:mo>+</m:mo><m:mn>6</m:mn><m:msub>
   <m:mrow>
    <m:mi>log</m:mi>
   </m:mrow>
   <m:mn>7</m:mn>
  </m:msub>
  <m:mi>w</m:mi><m:mo>- −</m:mo><m:mfrac>
   <m:mrow>
    <m:msub>
     <m:mrow>
      <m:mi>log</m:mi>
     </m:mrow>
     <m:mn>7</m:mn>
    </m:msub>

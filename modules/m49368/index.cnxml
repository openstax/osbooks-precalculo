   <m:mrow>
    <m:mn>3</m:mn><m:mi>x</m:mi>
   </m:mrow>
  </m:msup>
  <m:mo>⋅</m:mo><m:msup>
   <m:mrow>
    <m:mn>216</m:mn>
   </m:mrow>
   <m:mi>x</m:mi>
  </m:msup>
  <m:mo>=</m:mo><m:msup>
   <m:mrow>
    <m:mn>36</m:mn>
   </m:mrow>
   <m:mrow>

    <m:mtd columnalign="left">
     <m:mrow/>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mo>=</m:mo><m:mn>42,52722583</m:mn><m:mo>+</m:mo><m:mn>13,85752327</m:mn><m:mi>ln</m:mi><m:mo stretchy="false">(</m:mo><m:mn>14</m:mn><m:mo stretchy="false">)</m:mo>

  <m:mi>ln</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mi>z</m:mi>
  <m:mo>)</m:mo></m:mrow><m:mo>-</m:mo><m:mi>ln</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mi>x</m:mi>
  <m:mo>)</m:mo></m:mrow><m:mo>-</m:mo><m:mi>ln</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mi>y</m:mi>
  <m:mo>)</m:mo></m:mrow>
 </m:mrow>
</m:math> en forma compacta.</para>
</problem>
<solution id="fs-id1544533">
<para id="fs-id1676007"> <m:math>
 <m:mrow>
  <m:mi>ln</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mfrac>
     <m:mi>z</m:mi>
     <m:mrow>
      <m:mi>x</m:mi><m:mi>y</m:mi>
     </m:mrow>
    </m:mfrac>
   </m:mrow>
  <m:mo>)</m:mo></m:mrow>
 </m:mrow>
</m:math> </para>
</solution>

       <m:mn>6</m:mn>
      </m:msup>

     </m:mrow>
     <m:mrow>
      <m:mroot>
       <m:mi>u</m:mi>
       <m:mn>3</m:mn>
      </m:mroot>

     </m:mrow>
    </m:mfrac>

   </m:mrow>
  <m:mo>)</m:mo></m:mrow>
 </m:mrow>

<exercise id="fs-id1508077">
<problem id="fs-id1430560">
<para id="fs-id1430562">Reescriba <m:math>
 <m:mrow>
  <m:msub>
   <m:mrow>
    <m:mi>log</m:mi></m:mrow>
   <m:mi>t</m:mi>
  </m:msub>
  <m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mn>96</m:mn></m:mrow>
  <m:mo>)</m:mo></m:mrow><m:mo>-</m:mo><m:msub>
   <m:mrow>
    <m:mi>log</m:mi></m:mrow>
   <m:mi>t</m:mi>
  </m:msub>
  <m:mrow><m:mo>(</m:mo>
   <m:mn>8</m:mn>
  <m:mo>)</m:mo></m:mrow></m:mrow>

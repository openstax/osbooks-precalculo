      <m:mi>c</m:mi><m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mn>1</m:mn><m:mo>+</m:mo><m:mi>a</m:mi><m:msup>
         <m:mi>e</m:mi>
         <m:mrow>
          <m:mo>&#8722;</m:mo><m:mi>b</m:mi><m:mi>t</m:mi>
         </m:mrow>
        </m:msup>

       </m:mrow>
      <m:mo>)</m:mo></m:mrow><m:mo>&#8722;</m:mo><m:mi>c</m:mi>
     </m:mrow>
     <m:mrow>
      <m:mn>1</m:mn><m:mo>+</m:mo><m:mi>a</m:mi><m:msup>
       <m:mi>e</m:mi>
       <m:mrow>
        <m:mo>&#8722;</m:mo><m:mi>b</m:mi><m:mi>t</m:mi>
       </m:mrow>
      </m:msup>

     </m:mrow>
    </m:mfrac>


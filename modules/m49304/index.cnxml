<m:mrow>
<m:mo stretchy="false">(</m:mo><m:mn>2 </m:mn><m:mo>,</m:mo><m:mn>8</m:mn><m:mo stretchy="false">]</m:mo><m:mo>,</m:mo></m:mrow>
</m:math> rango <m:math>
<m:mrow>
<m:mo stretchy="false">[</m:mo><m:mn>6</m:mn><m:mo>,</m:mo><m:mn>8</m:mn><m:mo stretchy="false">)</m:mo></m:mrow>
</m:math>
</para>
</solution>

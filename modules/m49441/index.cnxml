     </m:msup>
     <m:mo>+</m:mo><m:mn>3</m:mn><m:mi>x</m:mi><m:mo>-</m:mo><m:mn>2 </m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
    </m:mrow>
   </m:math>
   </para></problem></exercise>
   <exercise id="fs-id1794701" class="material-set-2"><problem id="fs-id1794702"><para id="fs-id1794703"> <m:math>
    <m:mrow>
     <m:msup>
      <m:mi>x</m:mi>
      <m:mn>2 </m:mn>
     </m:msup>
     <m:mo>+</m:mo><m:mn>4</m:mn><m:mi>x</m:mi><m:mi>y</m:mi><m:mo>+</m:mo><m:msup>
      <m:mi>y</m:mi>
      <m:mn>2 </m:mn>
     </m:msup>
     <m:mo>-</m:mo><m:mn>2 </m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
    </m:mrow>
   </m:math>
   </para></problem>
    <solution id="fs-id1685133">
     <para id="fs-id1685135"> <m:math>
      <m:mrow>
       <m:mi>θ</m:mi><m:mo>=</m:mo><m:msup>
        <m:mrow>
         <m:mn>45</m:mn>
        </m:mrow>
        <m:mo>∘</m:mo>
       </m:msup>
       <m:mo>,</m:mo><m:mn>3</m:mn><m:msup>
        <m:msup>
         <m:mi>x</m:mi>
         <m:mo>′</m:mo>
        </m:msup>

        <m:mn>2</m:mn>
       </m:msup>
       <m:mo>-</m:mo><m:msup>
        <m:msup>
         <m:mi>y</m:mi>
         <m:mo>′</m:mo>
        </m:msup>

        <m:mn>2</m:mn>
       </m:msup>
       <m:mo>-</m:mo><m:msqrt>
        <m:mn>2 </m:mn>
       </m:msqrt>
       <m:msup>
        <m:mi>x</m:mi>
        <m:mo>′</m:mo>
       </m:msup>
       <m:mo>+</m:mo><m:msqrt>
        <m:mn>2 </m:mn>
       </m:msqrt>
       <m:msup>
        <m:mi>y</m:mi>
        <m:mo>′</m:mo>
       </m:msup>
       <m:mo>+</m:mo><m:mn>1</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
      </m:mrow>
     </m:math>
     </para>
    </solution></exercise>
   <exercise id="fs-id1266703" class="material-set-2"><problem id="fs-id1266704"><para id="fs-id1266705"> <m:math>

   <exercise id="fs-id1753645"><problem id="fs-id1753646"><para id="fs-id1753647"> <m:math>
 <m:mrow>
  <m:mn>2 </m:mn><m:msup>
   <m:mi>x</m:mi>
   <m:mn>2 </m:mn>
  </m:msup>
  <m:mo>+</m:mo><m:mn>8</m:mn><m:mi>x</m:mi><m:mi>y</m:mi><m:mo>-</m:mo><m:mn>1</m:mn><m:mo>=</m:mo><m:mn>0</m:mn><m:mo>,</m:mo><m:mi>θ</m:mi><m:mo>=</m:mo><m:mn>30°</m:mn>
 </m:mrow>
</m:math>
</para></problem></exercise>
   <exercise id="fs-id1558064"><problem id="fs-id1558065"><para id="fs-id1558066"> <m:math>
 <m:mrow>
  <m:mo>-</m:mo><m:mn>2 </m:mn><m:msup>
   <m:mi>x</m:mi>
   <m:mn>2 </m:mn>
  </m:msup>
  <m:mo>+</m:mo><m:mn>8</m:mn><m:mi>x</m:mi><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>1</m:mn><m:mo>=</m:mo><m:mn>0</m:mn><m:mo>,</m:mo><m:mi>θ</m:mi><m:mo>=</m:mo><m:mn>45°</m:mn>
 </m:mrow>
</m:math>
</para></problem>
    <solution id="fs-id1799395">
     <para id="fs-id1799397"> <m:math>
      <m:mrow>
       <m:mn>3</m:mn><m:msup>
        <m:msup>
         <m:mi>x</m:mi>
         <m:mo>′</m:mo>
        </m:msup>

        <m:mn>2</m:mn>
       </m:msup>
       <m:mo>+</m:mo><m:mn>2 </m:mn><m:msup>
        <m:mi>x</m:mi>
        <m:mo>′</m:mo>
       </m:msup>
       <m:msup>
        <m:mi>y</m:mi>
        <m:mo>′</m:mo>
       </m:msup>
       <m:mo>- −</m:mo><m:mn>5</m:mn><m:msup>
        <m:msup>
         <m:mi>y</m:mi>
         <m:mo>′</m:mo>
        </m:msup>

        <m:mn>2</m:mn>
       </m:msup>
       <m:mo>+</m:mo><m:mn>1</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
      </m:mrow>
     </m:math>
     </para></solution></exercise>
   <exercise id="fs-id1587288"><problem id="fs-id1587289"><para id="fs-id1587290"> <m:math>
 <m:mrow>
  <m:mn>4</m:mn><m:msup>

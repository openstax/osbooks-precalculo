       <entry align="center"><emphasis>parábola</emphasis></entry>
       <entry align="center"> <m:math>
 <m:mrow>
  <m:mi>A</m:mi><m:msup>
   <m:mi>x</m:mi>
   <m:mn>2 </m:mn>
  </m:msup>
  <m:mo>+</m:mo><m:mi>D</m:mi><m:mi>x</m:mi><m:mo>+</m:mo><m:mi>E</m:mi><m:mi>y</m:mi><m:mo>+</m:mo><m:mi>F</m:mi><m:mo>=</m:mo><m:mn>0</m:mn><m:mspace width="0.5em"/><m:mtext>o </m:mtext><m:mi>C</m:mi><m:msup>
   <m:mi>y</m:mi>
   <m:mn>2 </m:mn>
  </m:msup>
  <m:mo>+</m:mo><m:mi>D</m:mi><m:mi>x</m:mi><m:mo>+</m:mo><m:mi>E</m:mi><m:mi>y</m:mi><m:mo>+</m:mo><m:mi>F</m:mi><m:mo>=</m:mo><m:mn>0</m:mn>
 </m:mrow>
</m:math>
       </entry>
      </row>

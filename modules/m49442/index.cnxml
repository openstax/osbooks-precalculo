          </m:msup>
          <m:mo>=</m:mo><m:mfrac>
            <m:mn>1</m:mn>
            <m:mn>2 </m:mn>
          </m:mfrac>
          <m:mrow><m:mo>(</m:mo>
            <m:mrow>
              <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>3</m:mn>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow>

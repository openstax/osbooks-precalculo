<m:mn>–2</m:mn><m:mo>,</m:mo><m:mn>−2</m:mn><m:mo>+</m:mo><m:mn>4</m:mn><m:msqrt>
                <m:mn>3</m:mn>
              </m:msqrt>
              <m:mo>,</m:mo>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
              <m:mrow>
<m:mn>–2</m:mn><m:mo>,</m:mo><m:mn>−2</m:mn><m:mn>-4</m:mn><m:msqrt>
                  <m:mn>3</m:mn>
                </m:msqrt>

              </m:mrow>
              <m:mo>)</m:mo></m:mrow>
        </m:mrow>
      </m:math> <newline count="1"/></para><media id="fs-id1363974" alt="" display="block">

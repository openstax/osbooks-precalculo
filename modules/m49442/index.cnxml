      <exercise id="fs-id2875786"><problem id="fs-id2875787"><para id="fs-id2875788"> <m:math>
        <m:mrow>
          <m:mn>3</m:mn><m:msup>
            <m:mi>x</m:mi>
            <m:mn>2 </m:mn>
          </m:msup>
          <m:mo>-</m:mo><m:msup>
            <m:mi>y</m:mi>
            <m:mn>2 </m:mn>
          </m:msup>
          <m:mo>-</m:mo><m:mn>12</m:mn><m:mi>x</m:mi><m:mo>-</m:mo><m:mn>6</m:mn><m:mi>y</m:mi><m:mo>- −</m:mo><m:mn>9</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
        </m:mrow>
      </m:math>
      </para></problem>
    <solution id="fs-id2875843">
      <para id="fs-id2875845"> <m:math>
        <m:mrow>
          <m:mfrac>
            <m:mrow>
              <m:msup>
                <m:mrow>
                  <m:mrow><m:mo>(</m:mo>
                    <m:mrow>
                      <m:mi>x</m:mi><m:mo>-</m:mo><m:mn>2 </m:mn>
                    </m:mrow>
                    <m:mo>)</m:mo></m:mrow>
                </m:mrow>
                <m:mn>2</m:mn>
              </m:msup>

            </m:mrow>
            <m:mrow>
              <m:msup>
                <m:mn>2 </m:mn>
                <m:mn>2 </m:mn>
              </m:msup>

            </m:mrow>
          </m:mfrac>
          <m:mo>-</m:mo><m:mfrac>
            <m:mrow>
              <m:msup>
                <m:mrow>
                  <m:mrow><m:mo>(</m:mo>
                    <m:mrow>
                      <m:mi>y</m:mi><m:mo>+</m:mo><m:mn>3</m:mn>
                    </m:mrow>
                    <m:mo>)</m:mo></m:mrow>
                </m:mrow>
                <m:mn>2 </m:mn>
              </m:msup>

            </m:mrow>
            <m:mrow>
              <m:msup>
                <m:mrow>
                  <m:mrow><m:mo>(</m:mo>
                    <m:mrow>
                      <m:mn>2 </m:mn><m:msqrt>
                        <m:mn>3</m:mn>
                      </m:msqrt>

                    </m:mrow>
                    <m:mo>)</m:mo></m:mrow>
                </m:mrow>
                <m:mn>2 </m:mn>
              </m:msup>

            </m:mrow>
          </m:mfrac>
          <m:mo>=</m:mo><m:mn>1</m:mn><m:mo>;</m:mo>
        </m:mrow>
      </m:math> centro: <m:math>
        <m:mrow>
          <m:mrow><m:mo>(</m:mo>
            <m:mrow>
              <m:mn>2 </m:mn><m:mo>,</m:mo><m:mn>−3</m:mn>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow><m:mo>;</m:mo>
        </m:mrow>
      </m:math> vértices: <m:math>
        <m:mrow>
          <m:mrow><m:mo>(</m:mo>
            <m:mrow>
              <m:mn>4</m:mn><m:mo>,</m:mo><m:mn>−3</m:mn>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
              <m:mrow>
                <m:mn>0</m:mn><m:mo>,</m:mo><m:mn>−3</m:mn>
              </m:mrow>
              <m:mo>)</m:mo></m:mrow><m:mo>;</m:mo>
        </m:mrow>
      </m:math> focos: <m:math>
        <m:mrow>
          <m:mrow><m:mo>(</m:mo>
            <m:mrow>
              <m:mn>6</m:mn><m:mo>,</m:mo><m:mn>−3</m:mn>
            </m:mrow>
            <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
              <m:mrow>
                <m:mn>–2</m:mn><m:mo>,</m:mo><m:mn>−3</m:mn>
              </m:mrow>
              <m:mo>)</m:mo></m:mrow>
        </m:mrow>
      </m:math>
      </para></solution></exercise>


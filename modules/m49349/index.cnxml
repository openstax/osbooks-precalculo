   <exercise id="fs-id1165132940010"><problem id="fs-id1165132940011"><para id="fs-id1165132940012"> <m:math>
    <m:mrow>
     <m:msup>
      <m:mi>x</m:mi>
      <m:mn>3</m:mn>
     </m:msup>
     <m:mo>-</m:mo><m:mn>8</m:mn><m:msup>
      <m:mi>x</m:mi>
      <m:mn>2 </m:mn>
     </m:msup>
     <m:mo>+</m:mo><m:mn>25</m:mn><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>26</m:mn><m:mo>=</m:mo><m:mn>0</m:mn></m:mrow>
   </m:math>
   </para></problem>
    <solution id="fs-id1165137642786"><para id="fs-id1165137642787"> <m:math>
     <m:mrow>
      <m:mn>2</m:mn><m:mo>,</m:mo><m:mspace width="0.5em"/><m:mn>3</m:mn><m:mo>+</m:mo><m:mn>2 </m:mn><m:mi>i</m:mi><m:mo>,</m:mo><m:mspace width="0.5em"/><m:mn>3</m:mn><m:mo>-</m:mo><m:mn>2 </m:mn><m:mi>i</m:mi></m:mrow>
    </m:math>
    </para></solution></exercise>
   <exercise id="fs-id1165135199514"><problem id="fs-id1165135199515"><para id="fs-id1165135199516"> <m:math>

      <m:mo>-</m:mo><m:mfrac>
       <m:mn>2 </m:mn>
       <m:mn>3</m:mn>
      </m:mfrac>
      <m:mo>,</m:mo><m:mspace width="0.5em"/><m:mn>1</m:mn><m:mo>+</m:mo><m:mn>2 </m:mn><m:mi>i</m:mi><m:mo>,</m:mo><m:mspace width="0.5em"/><m:mn>1</m:mn><m:mo>-</m:mo><m:mn>2 </m:mn><m:mi>i</m:mi></m:mrow>
    </m:math>
    </para></solution></exercise>
   <exercise id="fs-id1165135416512" class="material-set-2"><problem id="fs-id1165135416513"><para id="fs-id1165135416514"> <m:math>
    <m:mrow>
     <m:msup>
      <m:mi>x</m:mi>
      <m:mn>4</m:mn>
     </m:msup>
     <m:mo>+</m:mo><m:mn>2 </m:mn><m:msup>

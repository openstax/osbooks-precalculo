      <m:mi>x</m:mi>
      <m:mn>3</m:mn>
     </m:msup>
     <m:mo>-</m:mo><m:mn>9</m:mn><m:msup>
      <m:mi>x</m:mi>
      <m:mn>2 </m:mn>
     </m:msup>
     <m:mo>+</m:mo><m:mn>13</m:mn><m:mi>x</m:mi><m:mo>-</m:mo><m:mn>6</m:mn><m:mo>;</m:mo><m:mspace width="0.5em"/><m:mi>x</m:mi><m:mo>–</m:mo><m:mn>1</m:mn></m:mrow>
   </m:math> </para></problem></exercise>

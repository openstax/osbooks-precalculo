      <m:mn>2 </m:mn>
     </m:msup>
     <m:mo>+</m:mo><m:mn>57</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>85</m:mn><m:mo>=</m:mo><m:mn>0</m:mn></m:mrow>
   </m:math>
   </para></problem></exercise>
   <exercise id="fs-id1165134380350" class="material-set-2"><problem id="fs-id1165134380351"><para id="fs-id1165134380352"> <m:math>
    <m:mrow>
     <m:mn>3</m:mn><m:msup>
      <m:mi>x</m:mi>
      <m:mn>3</m:mn>
     </m:msup>
     <m:mo>-</m:mo><m:mn>4</m:mn><m:msup>
      <m:mi>x</m:mi>
      <m:mn>2 </m:mn>
     </m:msup>
     <m:mo>+</m:mo><m:mn>11</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>10</m:mn><m:mo>=</m:mo><m:mn>0</m:mn></m:mrow>
   </m:math>

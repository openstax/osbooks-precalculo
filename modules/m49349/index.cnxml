      <m:mn>3</m:mn>
     </m:msup>
     <m:mo>+</m:mo><m:mn>5</m:mn><m:msup>
      <m:mi>x</m:mi>
      <m:mn>2 </m:mn>
     </m:msup>
     <m:mo>-</m:mo><m:mn>12</m:mn><m:mi>x</m:mi><m:mo>−</m:mo><m:mn>30</m:mn><m:mo>,</m:mo><m:mtext>​</m:mtext><m:mspace width="0.5em"/><m:mn>2 </m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>5</m:mn></m:mrow>
   </m:math>
   </para></problem>
    <solution id="fs-id1165134234213"><para id="fs-id1165134234214"> <m:math>
     <m:mrow>
      <m:mo>-</m:mo><m:mfrac>
       <m:mn>5</m:mn>
       <m:mn>2 </m:mn>
      </m:mfrac>
      <m:mo>,</m:mo><m:mspace width="0.5em"/><m:msqrt>
       <m:mn>6</m:mn>
      </m:msqrt>
      <m:mo>,</m:mo><m:mspace width="0.5em"/><m:mo>−</m:mo><m:msqrt>
       <m:mn>6</m:mn>
      </m:msqrt>
     </m:mrow>
    </m:math>
    </para></solution></exercise>



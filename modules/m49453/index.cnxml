   </m:mrow>
   <m:mrow>
    <m:mi>x</m:mi><m:mrow><m:mo stretchy="false">→</m:mo></m:mrow><m:mi>a</m:mi>
   </m:mrow>
  </m:munder>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>-</m:mo><m:munder>
   <m:mrow>
    <m:mi>lím</m:mi>
   </m:mrow>
   <m:mrow>
    <m:mi>x</m:mi><m:mrow><m:mo stretchy="false">→</m:mo></m:mrow><m:mi>a</m:mi>
   </m:mrow>
  </m:munder>
  <m:mi>g</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mi>A</m:mi><m:mo>-</m:mo><m:mi>B</m:mi>
 </m:mrow>
</m:math> </entry>
</row>
<row>
<entry align="center">Producto de funciones</entry>
<entry align="center"> <m:math>
 <m:mrow>
  <m:munder>
   <m:mrow>
    <m:mi>lím</m:mi>
   </m:mrow>
   <m:mrow>
    <m:mi>x</m:mi><m:mrow><m:mo stretchy="false">→</m:mo></m:mrow><m:mi>a</m:mi>
   </m:mrow>
  </m:munder>
  <m:mrow><m:mo>[</m:mo> <m:mrow>
   <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>⋅</m:mo><m:mi>g</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo>
  </m:mrow> <m:mo>]</m:mo></m:mrow><m:mo>=</m:mo><m:munder>
   <m:mrow>

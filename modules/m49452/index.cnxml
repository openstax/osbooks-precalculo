   <m:mrow>
    <m:mi>lím</m:mi>
   </m:mrow>
   <m:mrow>
    <m:mi>x</m:mi><m:mrow><m:mo stretchy="false">→</m:mo></m:mrow><m:msup>
     <m:mn>0</m:mn>
     <m:mo>+</m:mo>
    </m:msup>

   </m:mrow>
  </m:munder>
  <m:mspace width="0.5em"/><m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mo>–</m:mo><m:mn>3</m:mn></m:math>, <m:math><m:munder>
   <m:mrow>
    <m:mi>lím</m:mi>
   </m:mrow>
   <m:mrow>

<solution id="fs-id1165137556975">
<para id="fs-id1165137726425"> <m:math>
<m:mrow>
<m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mrow><m:mo>{</m:mo> <m:mrow>
<m:mtable>
<m:mtr>
<m:mtd>
<m:mrow>
<m:mrow><m:mo>|</m:mo> <m:mi>x</m:mi> <m:mo>|</m:mo></m:mrow><m:mspace width="0.5em"/><m:mtext>si</m:mtext><m:mspace width="0.5em"/><m:mi>x</m:mi><m:mo>≤</m:mo><m:mn>2 </m:mn></m:mrow>
</m:mtd>
</m:mtr>
<m:mtr>
<m:mtd>
<m:mrow>
<m:mn>3</m:mn><m:mspace width="0.5em"/><m:mtext>si</m:mtext><m:mspace width="0.5em"/><m:mi>x</m:mi><m:mo>&gt;</m:mo><m:mn>2 </m:mn></m:mrow>
</m:mtd>
</m:mtr>

</m:mtable></m:mrow> </m:mrow></m:mrow>
</m:math>
</para>
</solution>
</exercise>


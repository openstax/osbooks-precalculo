<m:mrow><m:mo>(</m:mo>
<m:mrow>
<m:mi>f</m:mi><m:mo>∘</m:mo><m:mi>g</m:mi></m:mrow>
<m:mo>)</m:mo></m:mrow><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mfrac>

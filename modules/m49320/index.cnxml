</problem>
<solution id="fs-id1165137635245">
<para id="fs-id1165137635247"> <m:math>
<m:mrow>
<m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mfrac>
<m:mn>1</m:mn>
<m:mn>2 </m:mn>
</m:mfrac>
<m:mrow><m:mo>|</m:mo> <m:mrow>
<m:mi>x</m:mi><m:mo>+</m:mo><m:mn>2 </m:mn></m:mrow> <m:mo>|</m:mo></m:mrow><m:mo>+</m:mo><m:mn>1</m:mn></m:mrow>
</m:math>
</para>
</solution>
</exercise><exercise id="fs-id1165137836804"><problem id="fs-id1165137836807">

<media id="fs-id1165137849182" alt="Gráfico de f(x)." display="block">

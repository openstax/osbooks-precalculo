</m:math> calcule <m:math>
<m:mrow>
<m:msup>
<m:mi>f</m:mi>
<m:mrow>
<m:mo>−</m:mo><m:mn>1</m:mn></m:mrow>
</m:msup>
<m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>.</m:mo></m:mrow>
</m:math>
</para>
</problem>
<solution id="fs-id1165137851399">
<para id="fs-id1165137851402"> <m:math>
<m:mrow>
<m:msup>
<m:mi>f</m:mi>
<m:mrow>
<m:mo>−</m:mo><m:mn>1</m:mn></m:mrow>
</m:msup>
<m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mo>-</m:mo><m:mfrac>
<m:mrow>
<m:mi>x</m:mi><m:mo>−</m:mo><m:mn>11</m:mn></m:mrow>
<m:mn>2 </m:mn>
</m:mfrac>
</m:mrow>
</m:math>
</para>
</solution>

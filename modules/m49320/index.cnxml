<m:mrow>
<m:msup>
<m:mi>f</m:mi>
<m:mrow>
<m:mo>−</m:mo><m:mn>1</m:mn></m:mrow>
</m:msup>
<m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mfrac>
<m:mrow>
<m:mo>-</m:mo><m:mn>2 </m:mn><m:mi>x</m:mi></m:mrow>
<m:mrow>
<m:mi>x</m:mi><m:mo>–</m:mo><m:mn>1</m:mn></m:mrow>
</m:mfrac>
</m:mrow>
</m:math>
</para>
</solution>
</exercise>
<exercise id="fs-id1165137541591">

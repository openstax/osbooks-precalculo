</solution>
</exercise>
<exercise id="fs-id1165137807042">
<problem id="fs-id1165137807044">
<para id="fs-id1165137601405"> <m:math>
<m:mrow>
<m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mo>-</m:mo><m:mfrac>
<m:mn>2 </m:mn>

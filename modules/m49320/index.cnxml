<m:mrow>
<m:mtext>números</m:mtext></m:mrow>
</m:math>
</para>
</solution>
</exercise>
<exercise id="fs-id1165137542465">
<problem id="fs-id1165134250812">
<para id="fs-id1165134250814"> <m:math>
<m:mrow>
<m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mo>-</m:mo><m:mfrac>
<m:mn>5</m:mn>
<m:mrow>
<m:msup>
<m:mi>x</m:mi>
<m:mn>3</m:mn>
</m:msup>
</m:mrow>
</m:mfrac>
<m:mo>+</m:mo><m:mn>9</m:mn><m:msup>
<m:mi>x</m:mi>
<m:mn>5</m:mn>
</m:msup>
</m:mrow>
</m:math>
</para>
</problem>
</exercise>
<exercise id="fs-id1165137770368">
<problem id="fs-id1165135536626">
<para id="fs-id1165135536628"> <m:math>
<m:mrow>
<m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mfrac>
<m:mn>1</m:mn>
<m:mi>x</m:mi>
</m:mfrac>

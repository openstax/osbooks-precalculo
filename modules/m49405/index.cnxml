      <m:mo>.</m:mo>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mspace width="0.5em"/><m:msup>
       <m:mi>a</m:mi>
       <m:mn>2</m:mn>
      </m:msup>

     <m:mrow/>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow/>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mtext>       </m:mtext><m:mo>=</m:mo><m:msup>
       <m:mrow>
        <m:mo stretchy="false">(</m:mo><m:mi>b</m:mi><m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>θ</m:mi><m:mo>- −</m:mo><m:mi>c</m:mi><m:mo stretchy="false">)</m:mo>
       </m:mrow>
       <m:mn>2 </m:mn>
      </m:msup>
      <m:mo>+</m:mo><m:msup>
       <m:mrow>
        <m:mo stretchy="false">(</m:mo><m:mi>b</m:mi><m:mi>sen</m:mi><m:mspace width="0.3em"/><m:mi>θ</m:mi><m:mo stretchy="false">)</m:mo>
       </m:mrow>
       <m:mn>2</m:mn>
      </m:msup>

     </m:mrow>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow/>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow/>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow/>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow/>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mtext>Sustituya </m:mtext><m:mo stretchy="false">(</m:mo><m:mi>b</m:mi><m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>θ</m:mi><m:mo stretchy="false">)</m:mo><m:mtext> para</m:mtext><m:mspace width="0.3em"/><m:mi>x</m:mi><m:mspace width="0.3em"/><m:mspace width="0.3em"/><m:mtext>y </m:mtext><m:mo stretchy="false">(</m:mo><m:mi>b</m:mi><m:mi>sen</m:mi><m:mspace width="0.3em"/><m:mi>θ</m:mi><m:mo stretchy="false">)</m:mo><m:mspace width="0.3em"/><m:mtext>para </m:mtext><m:mi>y</m:mi><m:mo>.</m:mo>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mtext>       </m:mtext><m:mo>=</m:mo><m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:msup>
         <m:mi>b</m:mi>
         <m:mn>2</m:mn>
        </m:msup>
        <m:msup>
         <m:mrow>
          <m:mi>cos</m:mi>
         </m:mrow>
         <m:mn>2</m:mn>
        </m:msup>
        <m:mi>θ</m:mi><m:mo>-</m:mo><m:mn>2</m:mn><m:mi>b</m:mi><m:mi>c</m:mi><m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>θ</m:mi><m:mo>+</m:mo><m:msup>
         <m:mi>c</m:mi>
         <m:mn>2</m:mn>
        </m:msup>

       </m:mrow>
      <m:mo>)</m:mo></m:mrow><m:mo>+</m:mo><m:msup>
       <m:mi>b</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:msup>
       <m:mrow>
        <m:mi>sen</m:mi>
       </m:mrow>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mi>θ</m:mi>
     </m:mrow>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow/>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow/>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow/>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow/>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mtext>Expanda el cuadrado perfecto</m:mtext><m:mo>.</m:mo>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mtext>       </m:mtext><m:mo>=</m:mo><m:msup>
       <m:mi>b</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:msup>
       <m:mrow>
        <m:mi>cos</m:mi>
       </m:mrow>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mi>θ</m:mi><m:mo>+</m:mo><m:msup>
       <m:mi>b</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:msup>
       <m:mrow>
        <m:mi>sen</m:mi>
       </m:mrow>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mi>θ</m:mi><m:mo>+</m:mo><m:msup>
       <m:mi>c</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>-</m:mo><m:mn>2</m:mn><m:mi>b</m:mi><m:mi>c</m:mi><m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>θ</m:mi>
     </m:mrow>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow/>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow/>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow/>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow/>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mtext>Agrupe los términos al destacar que </m:mtext><m:msup>
       <m:mrow>
        <m:mi>cos</m:mi>
       </m:mrow>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mi>θ</m:mi><m:mo>+</m:mo><m:msup>
       <m:mrow>
        <m:mi>sen</m:mi>
       </m:mrow>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mi>θ</m:mi><m:mo>=</m:mo><m:mn>1.</m:mn>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mtext>       </m:mtext><m:mo>=</m:mo><m:msup>
       <m:mi>b</m:mi>
       <m:mn>2 </m:mn>
      </m:msup>
      <m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:msup>
         <m:mrow>
          <m:mi>cos</m:mi>
         </m:mrow>
         <m:mn>2</m:mn>
        </m:msup>
        <m:mi>θ</m:mi><m:mo>+</m:mo><m:msup>
         <m:mrow>
          <m:mi>sen</m:mi>
         </m:mrow>
         <m:mn>2</m:mn>
        </m:msup>
        <m:mi>θ</m:mi>
       </m:mrow>
      <m:mo>)</m:mo></m:mrow><m:mo>+</m:mo><m:msup>
       <m:mi>c</m:mi>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mo>-</m:mo><m:mn>2</m:mn><m:mi>b</m:mi><m:mi>c</m:mi><m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>θ</m:mi>
     </m:mrow>
    </m:mtd>
    <m:mtd columnalign="left">

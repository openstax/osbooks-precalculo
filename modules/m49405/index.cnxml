      <exercise id="fs-id1165135329815">
        <problem id="fs-id1165135329817">
          <para id="fs-id1165135329819"> <m:math>
            <m:mrow>
              <m:mi>β</m:mi><m:mo>=</m:mo><m:mn>67°</m:mn><m:mo>,</m:mo><m:mi>a</m:mi><m:mo>=</m:mo><m:mn>49</m:mn><m:mo>,</m:mo><m:mi>b</m:mi><m:mo>=</m:mo><m:mn>38</m:mn></m:mrow>
          </m:math>
          </para></problem>
      </exercise>
      <exercise id="fs-id1165131940267">
        <problem id="fs-id1165131940269">
          <para id="fs-id1165131940271"> <m:math>
            <m:mrow>
              <m:mi>α</m:mi><m:mo>=</m:mo><m:mn>43,1°</m:mn><m:mo>,</m:mo><m:mi>a</m:mi><m:mo>=</m:mo><m:mn>184,2</m:mn><m:mo>,</m:mo><m:mi>b</m:mi><m:mo>=</m:mo><m:mn>242,8</m:mn></m:mrow>
          </m:math>
          </para></problem>
        <solution id="fs-id1165137938467">
          <para id="fs-id1165137938469">257,4 </para>
        </solution>
      </exercise>
      <exercise id="fs-id1165135538836">
        <problem id="fs-id1165135538838">
          <para id="fs-id1165135538840"> <m:math>
            <m:mrow>

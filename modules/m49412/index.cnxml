     <m:mrow>
      <m:mn>13</m:mn>
     </m:mrow>
    </m:msqrt>

   </m:mrow>
   <m:mrow>
    <m:mn>13</m:mn>
   </m:mrow>
  </m:mfrac>
  <m:mstyle mathvariant="bold"><m:mi>i</m:mi></m:mstyle><m:mo>+</m:mo><m:mfrac>
   <m:mrow>
    <m:mn>3</m:mn><m:msqrt>
     <m:mrow>
      <m:mn>13</m:mn>
     </m:mrow>
    </m:msqrt>

   </m:mrow>
   <m:mrow>
    <m:mn>13</m:mn>

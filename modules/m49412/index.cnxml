  <m:mi>x</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mi>t</m:mi>
  <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mi>a</m:mi><m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>t</m:mi></m:mrow>
</m:math> y <m:math>
 <m:mrow>
  <m:mi>y</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mstyle mathvariant="bold"><m:mi>b</m:mi></m:mstyle><m:mi>sen</m:mi><m:mspace width="0.3em"/><m:mi>t</m:mi></m:mrow>

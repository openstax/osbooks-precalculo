    <m:mo>-</m:mo><m:mn>1</m:mn><m:mo>,</m:mo><m:mo>-</m:mo><m:mn>1</m:mn></m:mrow>
  <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:msub>
   <m:mi>P</m:mi>

  </m:msub>
  <m:mo>=</m:mo><m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mn>3</m:mn><m:mo>,</m:mo><m:mo>-</m:mo><m:mn>2 </m:mn></m:mrow>
  <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:msub>

 <m:mrow>
  <m:mo>-</m:mo><m:mfrac>
   <m:mrow>
    <m:mn>7</m:mn><m:msqrt>
     <m:mn>2</m:mn>
    </m:msqrt>
    </m:mrow>
   <m:mrow>
    <m:mn>10</m:mn></m:mrow>
  </m:mfrac>

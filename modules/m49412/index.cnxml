<exercise id="fs-id1165134032413" class="material-set-2"><problem id="fs-id1165134032414"><para id="fs-id1165134573200"> <m:math>
 <m:mrow>
  <m:msub>
   <m:mi>P</m:mi>
   <m:mn>1</m:mn>
  </m:msub>
  <m:mo>=</m:mo><m:mrow><m:mo>(</m:mo>

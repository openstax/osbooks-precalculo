    <m:mrow>
     <m:mn>27</m:mn>
    </m:mrow>
    <m:mrow>
     <m:mn>12</m:mn>
    </m:mrow>
   </m:mfrac>
   <m:mo>,</m:mo><m:mo>−</m:mo><m:mfrac>
    <m:mrow>
     <m:mn>81</m:mn>
    </m:mrow>

   </m:mfrac>
   <m:mo>,</m:mo><m:mo> </m:mo><m:mn>1</m:mn><m:mo>,</m:mo><m:mo> </m:mo><m:mi>e</m:mi><m:mo>,</m:mo><m:mo> </m:mo><m:msup>
    <m:mi>e</m:mi>
    <m:mn>2</m:mn>

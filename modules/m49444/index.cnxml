       </m:mrow>
      </m:msub>
      <m:mtext>, para </m:mtext><m:mi>n</m:mi><m:mo>≥</m:mo><m:mn>4</m:mn>
     </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable>
 </m:mrow>
</m:math>
</equation></problem>
<solution id="fs-id1165135485815">
<para id="fs-id1165135485817"> <m:math>
 <m:mrow>
  <m:mrow><m:mo>{</m:mo> <m:mrow>
   <m:mn>0</m:mn><m:mo>,</m:mo><m:mo> </m:mo><m:mn>1</m:mn><m:mo>,</m:mo><m:mo> </m:mo><m:mn>1</m:mn><m:mo>,</m:mo><m:mo> </m:mo><m:mn>1</m:mn><m:mo>,</m:mo><m:mo> </m:mo><m:mn>2 </m:mn><m:mo>,</m:mo><m:mo> </m:mo><m:mn>3</m:mn><m:mo>,</m:mo><m:mo> </m:mo><m:mfrac>
    <m:mn>5</m:mn>
    <m:mn>2 </m:mn>
   </m:mfrac>
   <m:mo>,</m:mo><m:mspace width="0.5em"/><m:mtext/><m:mfrac>
    <m:mrow>
     <m:mn>17</m:mn>
    </m:mrow>
    <m:mn>6</m:mn>
   </m:mfrac>

  </m:mrow> <m:mo>}</m:mo></m:mrow><m:mo>.</m:mo>
 </m:mrow>
</m:math> </para>
</solution>
</exercise>
</note>

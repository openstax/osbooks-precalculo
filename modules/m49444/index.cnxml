       <m:mi>a</m:mi>
       <m:mn>2</m:mn>
      </m:msub>
      <m:mo>=</m:mo><m:mo>-</m:mo><m:mn>3</m:mn><m:mo stretchy="false">(</m:mo><m:mn>2 </m:mn><m:mo stretchy="false">)</m:mo><m:mo>+</m:mo><m:mn>8</m:mn><m:mo>=</m:mo><m:mn>2</m:mn>
     </m:mrow>
    </m:mtd>

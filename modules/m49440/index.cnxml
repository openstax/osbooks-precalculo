            <m:mn>2 </m:mn>
          </m:msup>
          <m:mo>+</m:mo><m:mn>12</m:mn><m:mi>x</m:mi><m:mo>-</m:mo><m:mn>6</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>21</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
        </m:mrow>
      </m:math>
      </para></problem>
        <solution id="fs-id2017604">
          <para id="fs-id2017606"> <m:math>
            <m:mrow>
              <m:msup>
                <m:mrow>
                  <m:mrow><m:mo>(</m:mo>
                    <m:mrow>
                      <m:mi>y</m:mi><m:mo>-</m:mo><m:mn>3</m:mn>
                    </m:mrow>
                    <m:mo>)</m:mo></m:mrow>
                </m:mrow>
                <m:mn>2 </m:mn>
              </m:msup>
              <m:mo>=</m:mo><m:mn>−12</m:mn><m:mrow><m:mo>(</m:mo>
                <m:mrow>
                  <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
                </m:mrow>
                <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mi>V</m:mi><m:mo>:</m:mo><m:mrow><m:mo>(</m:mo>
                  <m:mrow>
                    <m:mo>-</m:mo><m:mn>1</m:mn><m:mo>,</m:mo><m:mn>3</m:mn>
                  </m:mrow>
                  <m:mo>)</m:mo></m:mrow><m:mo>;</m:mo><m:mi>F</m:mi><m:mo>:</m:mo><m:mrow><m:mo>(</m:mo>
                    <m:mrow>
                      <m:mo>-</m:mo><m:mn>4</m:mn><m:mo>,</m:mo><m:mn>3</m:mn>
                    </m:mrow>
                    <m:mo>)</m:mo></m:mrow><m:mo>;</m:mo><m:mi>d</m:mi><m:mo>:</m:mo><m:mi>x</m:mi><m:mo>=</m:mo><m:mn>2 </m:mn>
            </m:mrow>
          </m:math>
          </para>
        </solution></exercise>


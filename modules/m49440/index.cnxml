            </m:msqrt>
            <m:mrow><m:mo>(</m:mo>
              <m:mrow>
                <m:mi>x</m:mi><m:mo>+</m:mo><m:msqrt>
                  <m:mn>2 </m:mn>
                </m:msqrt>

              </m:mrow>
              <m:mo>)</m:mo></m:mrow>
          </m:mrow>
        </m:math>
        </para>
</solution></exercise>



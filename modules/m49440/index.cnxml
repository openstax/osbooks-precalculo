                      <m:mo>,</m:mo><m:mn>1</m:mn>
                    </m:mrow>
                    <m:mo>)</m:mo></m:mrow><m:mo>;</m:mo><m:mi>d</m:mi><m:mo>:</m:mo><m:mi>x</m:mi><m:mo>=</m:mo><m:mfrac>

      <exercise id="fs-id1923618"><problem id="fs-id1923619"><para id="fs-id1923620"> <m:math>
        <m:mrow>
          <m:mn>5</m:mn><m:msup>
            <m:mi>x</m:mi>
            <m:mn>2 </m:mn>
          </m:msup>
          <m:mo>−</m:mo><m:mn>50</m:mn><m:mi>x</m:mi><m:mo>-</m:mo><m:mn>4</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>113</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
        </m:mrow>
      </m:math>
      </para></problem>
        <solution id="fs-id1261870">
          <para id="fs-id1261872"> <m:math>
            <m:mrow>
              <m:msup>
                <m:mrow>
                  <m:mrow><m:mo>(</m:mo>
                    <m:mrow>
                      <m:mi>x</m:mi><m:mo>-</m:mo><m:mn>5</m:mn>
                    </m:mrow>
                    <m:mo>)</m:mo></m:mrow>
                </m:mrow>
                <m:mn>2 </m:mn>
              </m:msup>
              <m:mo>=</m:mo><m:mfrac>
                <m:mn>4</m:mn>
                <m:mn>5</m:mn>
              </m:mfrac>
              <m:mrow><m:mo>(</m:mo>
                <m:mrow>
                  <m:mi>y</m:mi><m:mo>+</m:mo><m:mn>3</m:mn>
                </m:mrow>
                <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mi>V</m:mi><m:mo>:</m:mo><m:mrow><m:mo>(</m:mo>
                  <m:mrow>
                    <m:mn>5</m:mn><m:mo>,</m:mo><m:mo>-</m:mo><m:mn>3</m:mn>
                  </m:mrow>
                  <m:mo>)</m:mo></m:mrow><m:mo>;</m:mo><m:mi>F</m:mi><m:mo>:</m:mo><m:mrow><m:mo>(</m:mo>
                    <m:mrow>
                      <m:mn>5</m:mn><m:mo>,</m:mo><m:mo>−</m:mo><m:mfrac>
                        <m:mrow>
                          <m:mn>14</m:mn>
                        </m:mrow>
                        <m:mn>5</m:mn>
                      </m:mfrac>

                    </m:mrow>
                    <m:mo>)</m:mo></m:mrow><m:mo>;</m:mo><m:mi>d</m:mi><m:mo>:</m:mo><m:mi>y</m:mi><m:mo>=</m:mo><m:mo>-</m:mo><m:mfrac>
                      <m:mrow>
                        <m:mn>16</m:mn>
                      </m:mrow>
                      <m:mn>5</m:mn>
                    </m:mfrac>

            </m:mrow>
          </m:math>
          </para>
        </solution></exercise>


</m:mrow>
<m:mo>)</m:mo>
</m:mrow>
</m:mrow>
<m:mn>2 </m:mn>
</m:msup>
<m:mo>+</m:mo>
<m:mn>1</m:mn>
</m:mtd>
</m:mtr>
<m:mtr>
<m:mtd columnalign="right">
<m:mi>x</m:mi>
</m:mtd>
<m:mtd columnalign="left">
<m:mo>=</m:mo>
</m:mtd>
<m:mtd columnalign="left">
<m:mn>2 </m:mn>
<m:msup>
<m:mrow>

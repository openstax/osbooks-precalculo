                </m:mrow>
                <m:mn>2 </m:mn>
              </m:msup>
              <m:mo>=</m:mo><m:mfrac>
                <m:mn>4</m:mn>
                <m:mn>3</m:mn>
              </m:mfrac>
              <m:mrow><m:mo>(</m:mo>

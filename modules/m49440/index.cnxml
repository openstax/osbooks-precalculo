        <solution id="fs-id2438901">
          <para id="fs-id2438904"> <m:math>
            <m:mrow>
              <m:msup>
                <m:mrow>
                  <m:mrow><m:mo>(</m:mo>
                    <m:mrow>
                      <m:mi>x</m:mi><m:mo>-</m:mo><m:mn>2 </m:mn>
                    </m:mrow>
                    <m:mo>)</m:mo></m:mrow>
                </m:mrow>
                <m:mn>2 </m:mn>
              </m:msup>
              <m:mo>=</m:mo><m:mn>−2</m:mn><m:mrow><m:mo>(</m:mo>
                <m:mrow>
                  <m:mi>y</m:mi><m:mo>-</m:mo><m:mn>5</m:mn>
                </m:mrow>
                <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mi>V</m:mi><m:mo>:</m:mo><m:mrow><m:mo>(</m:mo>
                  <m:mrow>
                    <m:mn>2 </m:mn><m:mo>,</m:mo><m:mn>5</m:mn>
                  </m:mrow>
                  <m:mo>)</m:mo></m:mrow><m:mo>;</m:mo><m:mi>F</m:mi><m:mo>:</m:mo><m:mrow><m:mo>(</m:mo>
                    <m:mrow>
                      <m:mn>2 </m:mn><m:mo>,</m:mo><m:mfrac>

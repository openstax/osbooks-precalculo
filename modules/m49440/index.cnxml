                      </m:mfrac>

                    </m:mrow>
                    <m:mo>)</m:mo></m:mrow><m:mo>;</m:mo><m:mi>d</m:mi><m:mo>:</m:mo><m:mi>y</m:mi><m:mo>=</m:mo><m:mfrac>
                      <m:mrow>
                        <m:mn>11</m:mn>
                      </m:mrow>
                      <m:mn>2 </m:mn>
                    </m:mfrac>

            </m:mrow>
          </m:math>
          </para>
        </solution></exercise>


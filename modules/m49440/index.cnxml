<m:mrow>
<m:mn>0</m:mn>
<m:mo>-</m:mo>
<m:mn>2 </m:mn>
</m:mrow>
<m:mo>)</m:mo>
</m:mrow>
</m:mrow>
<m:mn>2 </m:mn>
</m:msup>
<m:mo>+</m:mo>
<m:mn>1</m:mn>
</m:mtd>
</m:mtr>
<m:mtr>

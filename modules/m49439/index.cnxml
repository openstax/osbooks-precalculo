<exercise id="fs-id1944544"><problem id="fs-id1944545"><para id="fs-id1944546"> <m:math>
 <m:mrow>
  <m:mo>-</m:mo><m:mn>4</m:mn><m:msup>
   <m:mi>x</m:mi>
   <m:mn>2 </m:mn>
  </m:msup>
  <m:mo>-</m:mo><m:mn>16</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:msup>
   <m:mi>y</m:mi>
   <m:mn>2 </m:mn>
  </m:msup>
  <m:mo>-</m:mo><m:mn>2</m:mn><m:mi>y</m:mi><m:mo>−</m:mo><m:mn>19</m:mn><m:mo>=</m:mo><m:mn>0</m:mn>
 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id1944603"><para id="fs-id1944604"> <m:math>
 <m:mrow>
  <m:mi>y</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mi>x</m:mi>
  <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>1</m:mn><m:mo>+</m:mo><m:mn>2 </m:mn><m:msqrt>
   <m:mrow>
    <m:msup>
     <m:mi>x</m:mi>

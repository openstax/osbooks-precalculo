   </m:mrow>
  </m:msqrt>
  <m:mo>,</m:mo><m:mi>y</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mi>x</m:mi>
  <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>1</m:mn><m:mo>-</m:mo><m:mn>2 </m:mn><m:msqrt>
   <m:mrow>
    <m:msup>
     <m:mi>x</m:mi>
     <m:mn>2 </m:mn>
    </m:msup>
    <m:mo>+</m:mo><m:mn>4</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>5</m:mn>

    <solution id="fs-id1165135486004"><para id="fs-id1165135486005"> <m:math>
 <m:mrow>
  <m:msup>
   <m:mi>x</m:mi>
   <m:mn>3</m:mn>
  </m:msup>
  <m:mo>-</m:mo><m:mn>9</m:mn><m:msup>
   <m:mi>x</m:mi>

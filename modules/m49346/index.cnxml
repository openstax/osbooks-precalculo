<exercise id="fs-id1165137823331"><problem id="fs-id1165137734444"><para id="fs-id1165137734445"> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mi>x</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:msup>
     <m:mi>x</m:mi>
     <m:mn>2 </m:mn>
    </m:msup>
    <m:mo>-</m:mo><m:mn>2 </m:mn><m:mi>x</m:mi><m:mo>-</m:mo><m:mn>8</m:mn>

             </m:msqrt>
            </m:mrow>
            <m:mn>2</m:mn>
           </m:mfrac>
          </m:mrow>
          <m:mo>)</m:mo></m:mrow><m:mo>+</m:mo><m:msup>
           <m:mrow>

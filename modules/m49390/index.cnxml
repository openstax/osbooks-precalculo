          <m:mrow>
           <m:mo>−</m:mo><m:mn>1</m:mn></m:mrow>
         </m:msup>
         <m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mfrac>
            <m:mrow>
             <m:msqrt>
              <m:mn>3</m:mn>
             </m:msqrt>
            </m:mrow>
            <m:mn>2 </m:mn>
           </m:mfrac>
          </m:mrow>
          <m:mo>)</m:mo></m:mrow><m:mo>-</m:mo><m:msup>
           <m:mrow>
            <m:mi>sen</m:mi></m:mrow>
           <m:mrow>
            <m:mo>−</m:mo><m:mn>1</m:mn></m:mrow>
          </m:msup>
         <m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mfrac>

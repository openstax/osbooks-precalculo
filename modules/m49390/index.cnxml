     <m:mrow>
      <m:mi>sen</m:mi>
     </m:mrow>
     <m:mrow>
      <m:mo>−</m:mo><m:mn>1</m:mn>
     </m:mrow>
    </m:msup>
    <m:mrow><m:mo>(</m:mo>
     <m:mrow>
      <m:mn>1</m:mn><m:mo>-</m:mo><m:mn>2 </m:mn><m:mi>x</m:mi>
     </m:mrow>
    <m:mo>)</m:mo></m:mrow>
   </m:mrow>
  <m:mo>)</m:mo></m:mrow>
 </m:mrow>
</m:math>
</para>
</problem>
<solution id="fs-id1165135349381">
<para id="fs-id1165135349383"> <m:math>
 <m:mrow>
  <m:msqrt>
   <m:mrow>
    <m:mn>1</m:mn><m:mo>-</m:mo><m:msup>
     <m:mrow>
      <m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mn>1</m:mn><m:mo>-</m:mo><m:mn>2 </m:mn><m:mi>x</m:mi>
       </m:mrow>
      <m:mo>)</m:mo></m:mrow>
     </m:mrow>
     <m:mn>2 </m:mn>
    </m:msup>

   </m:mrow>
  </m:msqrt>

 </m:mrow>
</m:math>
</para>
</solution>
</exercise>
<exercise id="fs-id1165135349434">

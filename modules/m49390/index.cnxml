            <m:mo>−</m:mo><m:mn>1</m:mn></m:mrow>
          </m:msup>
         <m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mfrac>
            <m:mrow>
             <m:msqrt>

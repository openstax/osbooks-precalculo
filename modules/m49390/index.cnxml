<exercise id="fs-id1165135317556">
<problem id="fs-id1165135317559">
<para id="fs-id1165135317561"> <m:math>
 <m:mrow>
  <m:mi>cos</m:mi><m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:msup>
     <m:mrow>
      <m:mi>tan</m:mi>
     </m:mrow>
     <m:mrow>
      <m:mo>-</m:mo><m:mn>1</m:mn>
     </m:mrow>
    </m:msup>
    <m:mrow><m:mo>(</m:mo>
     <m:mrow>
      <m:msup>
       <m:mi>x</m:mi>
       <m:mn>2 </m:mn>
      </m:msup>

     </m:mrow>
    <m:mo>)</m:mo></m:mrow>
   </m:mrow>
  <m:mo>)</m:mo></m:mrow>
 </m:mrow>
</m:math>
</para>
</problem>
<solution id="fs-id1165135317622">
<para id="fs-id1165135317624"> <m:math>
 <m:mrow>
  <m:mfrac>
   <m:mn>1</m:mn>
   <m:mrow>
    <m:msqrt>
     <m:mrow>
      <m:mn>1</m:mn><m:mo>+</m:mo><m:msup>
       <m:mi>x</m:mi>
       <m:mn>4</m:mn>
      </m:msup>

     </m:mrow>
    </m:msqrt>

   </m:mrow>
  </m:mfrac>

 </m:mrow>
</m:math>
</para>
</solution>
</exercise>

<para id="fs-id1165135317664">En los siguientes ejercicios, supongamos <m:math>
 <m:mrow>

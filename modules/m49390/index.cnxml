          <m:mrow>
           <m:mfrac>
            <m:mn>1</m:mn>
            <m:mn>2 </m:mn>
           </m:mfrac>
          </m:mrow>
          <m:mo>)</m:mo></m:mrow><m:mo>-</m:mo><m:msup>
           <m:mrow>
            <m:mi>sen</m:mi></m:mrow>

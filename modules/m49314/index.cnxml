   <exercise id="fs-id1165137731590"><problem id="fs-id1165137603675">
     <para id="fs-id1165137603678"> <m:math>
      <m:mrow>
       <m:mi>y</m:mi><m:mo>=</m:mo><m:mo>-</m:mo><m:mrow><m:mo>|</m:mo> <m:mrow>
        <m:mi>x</m:mi><m:mo>-</m:mo><m:mn>3</m:mn></m:mrow> <m:mo>|</m:mo></m:mrow><m:mo>-</m:mo><m:mn>2 </m:mn></m:mrow>

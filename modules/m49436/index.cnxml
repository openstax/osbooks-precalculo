            </m:mrow>
           </m:mtd>
          </m:mtr>

         </m:mtable>
       </m:mrow> <m:mo>|</m:mo></m:mrow>
       </m:mrow>
       <m:mrow>
        <m:mrow><m:mo>|</m:mo> <m:mrow>
         <m:mtable>
          <m:mtr>
           <m:mtd>
            <m:mrow>
             <m:msub>
              <m:mi>a</m:mi>

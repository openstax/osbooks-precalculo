       <m:mn>7</m:mn>
      </m:mtd>
     </m:mtr>
     <m:mtr columnalign="right">
      <m:mtd columnalign="right">
       <m:mrow>
        <m:mn>−5</m:mn>
       </m:mrow>
      </m:mtd>
     </m:mtr>
     <m:mtr columnalign="right">
      <m:mtd columnalign="right">
       <m:mn>0</m:mn>

      <m:mn>0</m:mn>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mn>1</m:mn>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mn>2</m:mn>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="right">
     <m:mtd columnalign="right">
      <m:mn>0</m:mn>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mn>0</m:mn>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mn>0</m:mn>
     </m:mtd>
    </m:mtr>

   </m:mtable><m:mrow><m:mo>|</m:mo><m:mrow>
   <m:mtable columnalign="right">

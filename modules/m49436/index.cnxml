  <exercise id="fs-id1436662"><problem id="fs-id1436663"><para id="fs-id1436664"> <m:math>
   <m:mtable columnalign="right">
    <m:mtr>
     <m:mtd>
      <m:mn>5</m:mn><m:mi>x</m:mi><m:mo>-</m:mo><m:mn>4</m:mn><m:mi>y</m:mi><m:mo>-</m:mo><m:mn>3</m:mn><m:mi>z</m:mi><m:mo>=</m:mo><m:mn>0</m:mn>
     </m:mtd>
    </m:mtr>
    <m:mtr>
     <m:mtd>
      <m:mn>2</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>2</m:mn><m:mi>z</m:mi><m:mo>=</m:mo><m:mn>0</m:mn>
     </m:mtd>
    </m:mtr>
    <m:mtr>
     <m:mtd>
      <m:mi>x</m:mi><m:mo>-</m:mo><m:mn>6</m:mn><m:mi>y</m:mi><m:mo>-</m:mo><m:mn>7</m:mn><m:mi>z</m:mi><m:mo>=</m:mo><m:mn>0</m:mn>
     </m:mtd>
    </m:mtr>
   </m:mtable>

  </m:math>
  </para></problem>
   <solution id="fs-id1615808">
    <para id="fs-id1615810"> <m:math>
     <m:mrow>
      <m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mi>x</m:mi><m:mo>,</m:mo><m:mfrac>
         <m:mrow>
          <m:mn>16</m:mn><m:mi>x</m:mi>
         </m:mrow>
         <m:mn>5</m:mn>
        </m:mfrac>
        <m:mo>−</m:mo><m:mfrac>
         <m:mrow>
          <m:mn>13</m:mn><m:mi>x</m:mi>
         </m:mrow>
         <m:mn>5</m:mn>
        </m:mfrac>

       </m:mrow>
       <m:mo>)</m:mo></m:mrow>
     </m:mrow>
    </m:math>
    </para>
   </solution></exercise>
  <exercise id="fs-id1537027"><problem id="fs-id1537028"><para id="fs-id1537029"> <m:math>
   <m:mtable columnalign="left">

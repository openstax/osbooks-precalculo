       <m:mn>1</m:mn>
      </m:msub>
      <m:msub>
       <m:mi>b</m:mi>
       <m:mn>2</m:mn>
      </m:msub>
      <m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:msub>
       <m:mi>a</m:mi>
       <m:mn>2</m:mn>
      </m:msub>
      <m:msub>
       <m:mi>c</m:mi>
       <m:mn>1</m:mn>
      </m:msub>
      <m:mo>-</m:mo><m:msub>
       <m:mi>a</m:mi>
       <m:mn>1</m:mn>
      </m:msub>
      <m:msub>
       <m:mi>c</m:mi>
       <m:mn>2</m:mn>
      </m:msub>

     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mtext>                       </m:mtext><m:mi>y</m:mi><m:mo>=</m:mo><m:mfrac>
       <m:mrow>
        <m:msub>
         <m:mi>a</m:mi>
         <m:mn>2</m:mn>
        </m:msub>
        <m:msub>
         <m:mi>c</m:mi>
         <m:mn>1</m:mn>
        </m:msub>

      </m:msub>
      <m:msub>
       <m:mi>a</m:mi>
       <m:mn>1</m:mn>
      </m:msub>
      <m:mi>x</m:mi><m:mo>-</m:mo><m:msub>
       <m:mi>b</m:mi>
       <m:mn>1</m:mn>
      </m:msub>
      <m:msub>
       <m:mi>a</m:mi>

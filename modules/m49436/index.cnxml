         <m:mn>2</m:mn>
        </m:msub>
        <m:msub>
         <m:mi>b</m:mi>
         <m:mn>1</m:mn>
        </m:msub>
        <m:mo>-</m:mo><m:msub>
         <m:mi>a</m:mi>
         <m:mn>1</m:mn>
        </m:msub>
        <m:msub>
         <m:mi>b</m:mi>
         <m:mn>2</m:mn>
        </m:msub>


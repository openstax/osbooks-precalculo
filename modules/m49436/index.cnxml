</para></problem>
   <solution id="fs-id1455837">
    <para id="fs-id1455840"> <m:math>
     <m:mrow>
      <m:mfrac>
       <m:mn>1</m:mn>
       <m:mrow>
        <m:mn>20</m:mn>
       </m:mrow>
      </m:mfrac>
      <m:mrow><m:mo>(</m:mo>
       <m:mrow>

    </m:mtr>
   </m:mtable>

  </m:math>
  </para></problem></exercise>
  <exercise id="fs-id1432229"><problem id="fs-id1432230"><para id="fs-id1432231"> <m:math>
   <m:mtable columnalign="left">
    <m:mtr>
     <m:mtd>
      <m:msup>
       <m:mi>y</m:mi>
       <m:mn>2 </m:mn>
      </m:msup>
      <m:mo>+</m:mo><m:msup>
       <m:mi>x</m:mi>
       <m:mn>2 </m:mn>
      </m:msup>
      <m:mo>=</m:mo><m:mn>25</m:mn>
     </m:mtd>
    </m:mtr>
    <m:mtr>
     <m:mtd>
      <m:msup>
       <m:mi>y</m:mi>
       <m:mn>2 </m:mn>
      </m:msup>
      <m:mo>-</m:mo><m:mn>2 </m:mn><m:msup>
       <m:mi>x</m:mi>
       <m:mn>2 </m:mn>
      </m:msup>
      <m:mo>=</m:mo><m:mn>1</m:mn>
     </m:mtd>
    </m:mtr>
   </m:mtable>

  </m:math>
  </para></problem>
   <solution id="fs-id1703246">
    <para id="fs-id1703248"> <m:math>
     <m:mrow>
      <m:mo stretchy="false">(</m:mo><m:mo>–</m:mo><m:mn>2 </m:mn><m:msqrt>
       <m:mn>2</m:mn>
      </m:msqrt>
      <m:mo>,</m:mo><m:mo>−</m:mo><m:msqrt>
       <m:mrow>
        <m:mn>17</m:mn>
       </m:mrow>
      </m:msqrt>
      <m:mo stretchy="false">)</m:mo><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mo>–</m:mo><m:mn>2 </m:mn><m:msqrt>
         <m:mn>2</m:mn>
        </m:msqrt>
        <m:mo>,</m:mo><m:msqrt>
         <m:mrow>
          <m:mn>17</m:mn>
         </m:mrow>
        </m:msqrt>

       </m:mrow>
       <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
        <m:mrow>
         <m:mn>2</m:mn><m:msqrt>
          <m:mn>2</m:mn>
         </m:msqrt>
         <m:mo>,</m:mo><m:mo>−</m:mo><m:msqrt>
          <m:mrow>
           <m:mn>17</m:mn>
          </m:mrow>
         </m:msqrt>

        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mn>2</m:mn><m:msqrt>
           <m:mn>2</m:mn>
          </m:msqrt>
          <m:mo>,</m:mo><m:msqrt>
           <m:mrow>
            <m:mn>17</m:mn>
           </m:mrow>
          </m:msqrt>

         </m:mrow>
         <m:mo>)</m:mo></m:mrow>
     </m:mrow>
    </m:math>
    </para>
</solution></exercise>


      <m:mn>4</m:mn><m:mi>x</m:mi><m:mo>-</m:mo><m:mn>5</m:mn><m:mi>y</m:mi><m:mo>=</m:mo><m:mo>-</m:mo><m:mn>50</m:mn>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="right">
    <m:mtd columnalign="right">
     <m:mrow>
      <m:mo>−</m:mo><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>2 </m:mn><m:mi>y</m:mi><m:mo>=</m:mo><m:mn>80</m:mn>
     </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable>
 </m:mrow>
</m:math>
</para></problem>
  <solution id="fs-id1440482">
   <para id="fs-id1440484"> <m:math>
    <m:mrow>
     <m:mrow><m:mo>(</m:mo>
      <m:mrow>
       <m:mn>100</m:mn><m:mo>,</m:mo><m:mn>90</m:mn>
      </m:mrow>
      <m:mo>)</m:mo></m:mrow>
    </m:mrow>
   </m:math>
   </para>
</solution></exercise>

      </m:mfrac>
      <m:mo>=</m:mo><m:mfrac>
       <m:mrow>
        <m:msub>
         <m:mi>a</m:mi>
         <m:mn>1</m:mn>
        </m:msub>
        <m:msub>
         <m:mi>c</m:mi>
         <m:mn>2</m:mn>
        </m:msub>
        <m:mo>-</m:mo><m:msub>
         <m:mi>a</m:mi>
         <m:mn>2</m:mn>
        </m:msub>
        <m:msub>
         <m:mi>c</m:mi>
         <m:mn>1</m:mn>
        </m:msub>

       </m:mrow>
       <m:mrow>
        <m:msub>
         <m:mi>a</m:mi>
         <m:mn>1</m:mn>
        </m:msub>
        <m:msub>
         <m:mi>b</m:mi>
         <m:mn>2 </m:mn>
        </m:msub>
        <m:mo>-</m:mo><m:msub>
         <m:mi>a</m:mi>
         <m:mn>2</m:mn>
        </m:msub>
        <m:msub>
         <m:mi>b</m:mi>
         <m:mn>1</m:mn>
        </m:msub>

       </m:mrow>
      </m:mfrac>
      <m:mo>=</m:mo><m:mfrac>
       <m:mrow>
        <m:mo>|</m:mo><m:mtable>
         <m:mtr>
          <m:mtd>
           <m:mrow>
            <m:msub>
             <m:mi>a</m:mi>
             <m:mn>1</m:mn>
            </m:msub>

           </m:mrow>
          </m:mtd>
          <m:mtd>
           <m:mrow>
            <m:msub>
             <m:mi>c</m:mi>
             <m:mn>1</m:mn>
            </m:msub>

           </m:mrow>
          </m:mtd>
         </m:mtr>
         <m:mtr>
          <m:mtd>
           <m:mrow>
            <m:msub>
             <m:mi>a</m:mi>
             <m:mn>2</m:mn>
            </m:msub>

           </m:mrow>
          </m:mtd>
          <m:mtd>
           <m:mrow>
            <m:msub>

      <m:mrow>
       <m:mn>2 </m:mn>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mn>1</m:mn>
     </m:mtd>
    </m:mtr>
    <m:mtr columnalign="right">
     <m:mtd columnalign="right">
      <m:mn>0</m:mn>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mrow>
       <m:mn>0</m:mn>
      </m:mrow>
     </m:mtd>
     <m:mtd columnalign="right">
      <m:mrow>
       <m:mo>-</m:mo><m:mn>1</m:mn>
      </m:mrow>
     </m:mtd>
    </m:mtr>

   </m:mtable>
  </m:mrow> <m:mo>]</m:mo></m:mrow>
 </m:mrow>
</m:math>
</equation><para id="fs-id1370716">Aumente <m:math>
 <m:mrow>
  <m:mi>A</m:mi> </m:mrow>
</m:math> con las dos primeras columnas.</para>
  <equation id="fs-id1504903" class="unnumbered"><label/> <m:math display="block">
 <m:mrow>
  <m:mi>A</m:mi><m:mo>=</m:mo><m:mrow><m:mo>[</m:mo> <m:mrow>
   <m:mtable>
    <m:mtr>

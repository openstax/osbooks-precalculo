    <m:mtable>
     <m:mtr>
      <m:mtd>
       <m:mrow>
        <m:msub>
         <m:mi>a</m:mi>
         <m:mn>1</m:mn>
        </m:msub>

       </m:mrow>
      </m:mtd>
     </m:mtr>
     <m:mtr>
      <m:mtd>
       <m:mrow>
        <m:msub>
         <m:mi>a</m:mi>

 <m:math><m:mfenced open="{" close=""><m:mtable columnalign="left"><m:mtr><m:mtd><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>2 </m:mn><m:mi>y</m:mi><m:mo> </m:mo><m:mn>3</m:mn><m:mi>z</m:mi><m:mo>=</m:mo><m:mo> </m:mo><m:mn>1</m:mn><m:mo> </m:mo></m:mtd></m:mtr><m:mtr><m:mtd><m:mi>x</m:mi><m:mo> </m:mo><m:mn>3</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mi>z</m:mi><m:mo>=</m:mo><m:mn>1</m:mn></m:mtd></m:mtr><m:mtr><m:mtd><m:mn>2 </m:mn><m:mi>x</m:mi><m:mo> </m:mo><m:mi>y</m:mi><m:mo> </m:mo><m:mn>2</m:mn><m:mi>z</m:mi><m:mo>=</m:mo><m:mn>2 </m:mn></m:mtd></m:mtr></m:mtable></m:mfenced><m:mtable><m:mtr><m:mtd><m:mo>(</m:mo><m:mn>1</m:mn><m:mo>)</m:mo></m:mtd></m:mtr><m:mtr><m:mtd><m:mo>(</m:mo><m:mn>2 </m:mn><m:mo>)</m:mo></m:mtd></m:mtr><m:mtr><m:mtd><m:mo>(</m:mo><m:mn>3</m:mn><m:mo>)</m:mo></m:mtd></m:mtr></m:mtable></m:math>

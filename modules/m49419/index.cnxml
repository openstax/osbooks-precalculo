     <m:mn>1</m:mn>
     <m:mn>7</m:mn>
    </m:mfrac>
    <m:mo>,</m:mo><m:mo>-</m:mo><m:mfrac>
     <m:mn>3</m:mn>
     <m:mn>7</m:mn>

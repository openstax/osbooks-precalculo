<tbody>
<row>
<entry align="center"><emphasis> <m:math>
<m:mi>x</m:mi>
</m:math> </emphasis></entry>
<entry align="center"> <m:math>

<m:mo>-</m:mo><m:mn>3</m:mn>

</m:math> </entry>
<entry align="center"> <m:math>

<m:mo>-</m:mo><m:mn>2 </m:mn>

</m:math> </entry>
<entry align="center"> <m:math>

<m:mo>-</m:mo><m:mn>1</m:mn>

</m:math> </entry>
<entry align="center"> <m:math>
<m:mn>0</m:mn>
</m:math> </entry>
<entry align="center"> <m:math>
<m:mn>1</m:mn>
</m:math> </entry>
<entry align="center"> <m:math>
<m:mn>2 </m:mn>
</m:math> </entry>
<entry align="center"> <m:math>
<m:mn>3</m:mn>
</m:math> </entry>
</row>
<row>
<entry align="center"> <m:math display="block">
 <m:mstyle fontstyle="italic" fontweight="bold">
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo></m:mstyle>
<m:mstyle fontweight="bold"><m:mo>=</m:mo><m:mn>4</m:mn></m:mstyle>

    <m:mo>(</m:mo>
     <m:mstyle fontweight="bold"> <m:mfrac>
       <m:mn>1</m:mn>
       <m:mn>2 </m:mn>
      </m:mfrac></m:mstyle>
    <m:msup><m:mo>)</m:mo>


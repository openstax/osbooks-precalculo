<para id="fs-id1165135190292"> <m:math>
<m:mrow>
<m:mo>-</m:mo><m:mn>50</m:mn><m:mo>=</m:mo><m:mo>-</m:mo><m:msup>
<m:mrow>
<m:mrow><m:mo>(</m:mo>
<m:mrow>
<m:mfrac>
<m:mn>1</m:mn>
<m:mn>2 </m:mn>
</m:mfrac>

</m:mrow>
<m:mo>)</m:mo></m:mrow>
</m:mrow>
<m:mrow>
<m:mo>-</m:mo><m:mi>x</m:mi>
</m:mrow>
</m:msup>

</m:mrow>
</m:math> </para>
</problem>
</exercise>

<exercise id="fs-id1165137476627">
<problem id="fs-id1165137476629">
<para id="fs-id1165137476631"> <m:math>
<m:mrow>

            </m:mrow>
           </m:mfrac>

          </m:mrow>
         <m:mo>)</m:mo></m:mrow>
        </m:mrow> <m:mo>]</m:mo></m:mrow>
       </m:mrow>
       <m:mrow>
        <m:mo stretchy="false">(</m:mo><m:mn>2 </m:mn><m:mo>-</m:mo><m:mo stretchy="false">(</m:mo><m:mi>a</m:mi><m:mo>+</m:mo><m:mi>h</m:mi><m:mo stretchy="false">)</m:mo><m:mo stretchy="false">)</m:mo><m:mo stretchy="false">(</m:mo><m:mn>2 </m:mn><m:mo>-</m:mo><m:mi>a</m:mi><m:mo stretchy="false">)</m:mo><m:mo stretchy="false">(</m:mo><m:mi>h</m:mi><m:mo stretchy="false">)</m:mo>
       </m:mrow>
      </m:mfrac>

     </m:mrow>
    </m:mtd>
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mtext>Multiplique el numerador y el denominador por </m:mtext><m:mo stretchy="false">(</m:mo><m:mn>2 </m:mn><m:mo>-</m:mo><m:mo stretchy="false">(</m:mo><m:mi>a</m:mi><m:mo>+</m:mo><m:mi>h</m:mi><m:mo stretchy="false">)</m:mo><m:mo stretchy="false">)</m:mo><m:mo stretchy="false">(</m:mo><m:mn>2 </m:mn><m:mo>-</m:mo><m:mi>a</m:mi><m:mo stretchy="false">)</m:mo>
     </m:mrow>

       </m:mrow>
       <m:mrow>
        <m:menclose notation="updiagonalstrike">
         <m:mi>h</m:mi>
        </m:menclose>

       </m:mrow>
      </m:mfrac>

     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mtext>        </m:mtext><m:mo>=</m:mo><m:munder>
       <m:mrow>
        <m:mi>lím</m:mi>
       </m:mrow>
       <m:mrow>
        <m:mi>h</m:mi><m:mrow><m:mo stretchy="false">→</m:mo></m:mrow><m:mn>0</m:mn>
       </m:mrow>
      </m:munder>
      <m:mo stretchy="false">(</m:mo><m:mo>−</m:mo><m:mn>32</m:mn><m:mi>a</m:mi><m:mo>−</m:mo><m:mn>16</m:mn><m:mi>h</m:mi><m:mo>+</m:mo><m:mn>36</m:mn><m:mo stretchy="false">)</m:mo>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">

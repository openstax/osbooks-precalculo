        <m:mo>)</m:mo></m:mrow><m:mo>-</m:mo><m:mo stretchy="false">(</m:mo><m:mn>2 </m:mn><m:mo>-</m:mo><m:mo stretchy="false">(</m:mo><m:mi>a</m:mi><m:mo>+</m:mo><m:mi>h</m:mi><m:mo stretchy="false">)</m:mo><m:mo stretchy="false">)</m:mo><m:menclose notation="updiagonalstrike">
         <m:mrow>
          <m:mrow><m:mo>(</m:mo>

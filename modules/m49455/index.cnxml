<note id="ti_12_04_03" class="precalculus try">
 <label>Ejercicio</label>
<exercise id="fs-id1165137936765">
<problem id="fs-id1165134061601"><para id="fs-id1165134061602">Halle la derivada de la función <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mfrac>
   <m:mrow>
    <m:mn>10</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>11</m:mn>
   </m:mrow>
   <m:mrow>
    <m:mn>5</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>4</m:mn>
   </m:mrow>
  </m:mfrac>

 </m:mrow>
</m:math> en <m:math>
 <m:mrow>
  <m:mi>x</m:mi><m:mo>=</m:mo><m:mi>a</m:mi><m:mo>.</m:mo>
 </m:mrow>
</m:math> </para></problem>

<solution id="fs-id1165137640855">
<para id="fs-id1165137530308"> <m:math>
 <m:mrow>
  <m:msup>
   <m:mi>f</m:mi>
   <m:mo>′</m:mo>
  </m:msup>
  <m:mo stretchy="false">(</m:mo><m:mi>a</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mfrac>
   <m:mrow>
    <m:mo>-</m:mo><m:mn>15</m:mn>
   </m:mrow>
   <m:mrow>
    <m:msup>
     <m:mrow>
      <m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mn>5</m:mn><m:mi>a</m:mi><m:mo>+</m:mo><m:mn>4</m:mn>
       </m:mrow>
      <m:mo>)</m:mo></m:mrow>
     </m:mrow>
     <m:mn>2 </m:mn>
    </m:msup>

   </m:mrow>
  </m:mfrac>

 </m:mrow>
</m:math> </para>
</solution>
</exercise>
</note>

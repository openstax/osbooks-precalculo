   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:msup>
       <m:mi>s</m:mi>
       <m:mo>′</m:mo>
      </m:msup>
      <m:mo stretchy="false">(</m:mo><m:mi>a</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mo>−</m:mo><m:mn>32</m:mn><m:mi>a</m:mi><m:mo>+</m:mo><m:mn>36</m:mn>
     </m:mrow>
    </m:mtd>
   </m:mtr>

<solution id="eip-id1165134258497">
<para id="eip-id1165134258499"> <m:math>
 <m:mrow>
  <m:msup>
   <m:mi>f</m:mi>
   <m:mo>′</m:mo>
  </m:msup>
  <m:mrow><m:mo>(</m:mo>
   <m:mrow>
    <m:mo>-</m:mo><m:mn>1</m:mn>
   </m:mrow>
  <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>9</m:mn>
 </m:mrow>
</m:math> </para>
</solution>
</exercise><exercise id="fs-id1165135699042">

        <m:mo>)</m:mo></m:mrow>
       </m:mrow>
       <m:mrow>
        <m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mn>2 </m:mn><m:mo>-</m:mo><m:mrow><m:mo>(</m:mo>

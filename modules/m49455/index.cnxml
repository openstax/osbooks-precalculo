        </m:msup>
        <m:mo>+</m:mo><m:mn>2 </m:mn><m:mi>h</m:mi><m:mo>−</m:mo><m:mi>a</m:mi><m:mi>h</m:mi><m:mo>−</m:mo><m:mn>6</m:mn><m:mo>+</m:mo><m:mn>3</m:mn><m:mi>a</m:mi><m:mo>+</m:mo><m:mn>3</m:mn><m:mi>h</m:mi><m:mo>−</m:mo><m:mn>2 </m:mn><m:mi>a</m:mi><m:mo>+</m:mo><m:msup>
         <m:mi>a</m:mi>

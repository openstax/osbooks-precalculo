          <m:mo>-</m:mo><m:mn>36</m:mn><m:mi>a</m:mi>
         </m:mrow>
        </m:menclose>
        <m:menclose notation="updiagonalstrike">
         <m:mrow>
          <m:mo>−</m:mo><m:mn>200</m:mn>
         </m:mrow>
        </m:menclose>

       </m:mrow>
       <m:mi>h</m:mi>
      </m:mfrac>

     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mtext>        </m:mtext><m:mo>=</m:mo><m:munder>
       <m:mrow>
        <m:mi>lím</m:mi>
       </m:mrow>

<image mime-type="image/jpg" src="../../media/CNX_Precalc_Figure_03_07_221.jpg"/>
</media>
</problem>
<solution id="fs-id1165137414994"><para id="fs-id1165137414995"> <m:math>
 <m:mrow>
  <m:mi>y</m:mi><m:mo>=</m:mo><m:mfrac>
   <m:mn>1</m:mn>
   <m:mn>3</m:mn>
  </m:mfrac>
  <m:mfrac>
   <m:mrow>
    <m:msup>
     <m:mi>x</m:mi>
     <m:mn>2 </m:mn>

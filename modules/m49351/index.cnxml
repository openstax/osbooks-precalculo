<colspec colnum="3" colname="c3"/>
<colspec colnum="4" colname="c4"/>
<colspec colnum="5" colname="c5"/>
<tbody>
<row>
<entry align="center"><emphasis> <m:math>
 <m:mi>x</m:mi>
</m:math> </emphasis></entry>
<entry align="center">0,1</entry>
<entry align="center">0,01</entry>
<entry align="center">0,001</entry>
<entry align="center">0,0001</entry>
</row>
<row>
<entry align="center"><emphasis> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mfrac>
   <m:mn>1</m:mn>
   <m:mi>x</m:mi>
  </m:mfrac>

 </m:mrow>
</m:math> </emphasis></entry>
<entry align="center">10</entry>
<entry align="center">100</entry>

<exercise id="fs-id1165137668139">
<problem id="fs-id1165135667825">
<para id="fs-id1165135667827"> <m:math>
 <m:mrow>
  <m:mo>{</m:mo><m:mtable columnalign="left">
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mi>x</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>4</m:mn><m:mtext>cos</m:mtext><m:mspace width="0.3em"/><m:mi>t</m:mi>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mi>y</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>5</m:mn><m:mi>sen</m:mi><m:mspace width="0.3em"/><m:mi>t</m:mi><m:mspace width="0.5em"/>
     </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable>
 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id1165137762341">
<para id="fs-id1165137762343"> <m:math>
 <m:mrow>
  <m:msup>
   <m:mrow>
    <m:mrow><m:mo>(</m:mo>
     <m:mrow>
      <m:mfrac>
       <m:mi>x</m:mi>
       <m:mn>4</m:mn>
      </m:mfrac>
      </m:mrow>
    <m:mo>)</m:mo></m:mrow></m:mrow>
   <m:mn>2 </m:mn>
  </m:msup>
  <m:mo>+</m:mo><m:msup>
   <m:mrow>
    <m:mrow><m:mo>(</m:mo>
     <m:mrow>
      <m:mfrac>
       <m:mi>y</m:mi>
       <m:mn>5</m:mn>
      </m:mfrac>
      </m:mrow>
    <m:mo>)</m:mo></m:mrow></m:mrow>
   <m:mn>2 </m:mn>
  </m:msup>
  <m:mo>=</m:mo><m:mn>1</m:mn></m:mrow>
</m:math>
</para>
</solution>

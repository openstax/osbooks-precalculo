<exercise id="fs-id1165135384981">
<problem id="fs-id1165135384983">
<para id="fs-id1165133210007"> <m:math>
 <m:mrow>
  <m:mo>{</m:mo><m:mtable columnalign="left">
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mi>x</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>2</m:mn><m:msup>
       <m:mrow>
        <m:mtext>cos</m:mtext>
       </m:mrow>
       <m:mn>2</m:mn>
      </m:msup>
      <m:mi>t</m:mi>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mi>y</m:mi><m:mo stretchy="false">(</m:mo><m:mi>t</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mo>-</m:mo><m:mi>sen</m:mi><m:mspace width="0.3em"/><m:mi>t</m:mi><m:mspace width="0.5em"/>
     </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable>
 </m:mrow>
</m:math>
</para></problem>
<solution id="fs-id1165137602808">
<para id="fs-id1165137602810"> <m:math>
 <m:mrow>
  <m:msup>
   <m:mi>y</m:mi>
   <m:mn>2 </m:mn>
  </m:msup>
  <m:mo>=</m:mo><m:mn>1</m:mn><m:mo>-</m:mo><m:mfrac>
   <m:mn>1</m:mn>
   <m:mn>2 </m:mn>
  </m:mfrac>
  <m:mi>x</m:mi></m:mrow>
</m:math>
</para>
</solution>
</exercise>
<exercise id="fs-id1165135189886">
<problem id="fs-id1165135189889">
<para id="fs-id1165135189891"> <m:math>
 <m:mrow>
  <m:mrow><m:mo>{</m:mo> <m:mtable columnalign="left">
   <m:mtr>

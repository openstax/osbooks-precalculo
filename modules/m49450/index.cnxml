<m:mn>1</m:mn>
<m:mn>5</m:mn>
</m:mfrac>
<m:mo>,</m:mo><m:mspace width="0.5em"/><m:mtext/><m:mo>-</m:mo><m:mfrac>
<m:mn>1</m:mn>
<m:mrow>
<m:mn>15</m:mn>
</m:mrow>
</m:mfrac>
<m:mo>,</m:mo><m:mspace width="0.5em"/><m:mtext/><m:mo>-</m:mo><m:mfrac>

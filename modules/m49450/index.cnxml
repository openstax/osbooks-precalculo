<exercise id="fs-id1690786">
<problem id="fs-id1690788">
<para id="fs-id1690790">Calcule la suma de la serie geométrica infinita <m:math>
 <m:mrow>
  <m:munderover>
   <m:mstyle mathsize="140%" displaystyle="true"><m:mo>∑</m:mo></m:mstyle>
   <m:mrow>
    <m:mi>k</m:mi><m:mo>=</m:mo><m:mn>1</m:mn>
   </m:mrow>
   <m:mi>∞</m:mi>
  </m:munderover>
  <m:mn>45</m:mn><m:mo>⋅</m:mo><m:msup>
   <m:mrow>
    <m:mo stretchy="false">(</m:mo><m:mo>-</m:mo><m:mfrac>
     <m:mn>1</m:mn>
     <m:mn>3</m:mn>
    </m:mfrac>
    <m:mo stretchy="false">)</m:mo>
   </m:mrow>
   <m:mrow>
    <m:mi>k</m:mi><m:mo>-</m:mo><m:mn>1</m:mn>
   </m:mrow>
  </m:msup>

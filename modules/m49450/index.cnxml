<m:mn>3</m:mn>
</m:mfrac>

</m:mrow>
<m:mo>)</m:mo></m:mrow>
</m:mrow>
<m:mrow>
<m:mi>n</m:mi><m:mo>–</m:mo><m:mn>1</m:mn>
</m:mrow>
</m:msup>

</m:mrow>
</m:math>

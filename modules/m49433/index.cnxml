         <m:mtd>
          <m:mi>h</m:mi>
         </m:mtd>
        </m:mtr>

       </m:mtable>
      </m:mrow> <m:mo>]</m:mo></m:mrow>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mspace width="0.5em"/><m:mtext>        </m:mtext><m:mo>=</m:mo><m:mrow><m:mo>[</m:mo> <m:mrow>
       <m:mtable>
        <m:mtr>
         <m:mtd>
          <m:mrow>
           <m:mi>a</m:mi><m:mo>+</m:mo><m:mi>e</m:mi>
          </m:mrow>
         </m:mtd>

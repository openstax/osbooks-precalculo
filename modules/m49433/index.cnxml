    <m:mtd columnalign="left">
     <m:mrow>
      <m:mo>=</m:mo><m:mrow><m:mo>[</m:mo> <m:mrow>
       <m:mtable columnalign="right">
        <m:mtr columnalign="right">
         <m:mtd columnalign="right">
          <m:mrow>
           <m:mn>−1</m:mn><m:mo stretchy="false">(</m:mo><m:mn>5</m:mn><m:mo stretchy="false">)</m:mo><m:mo>+</m:mo><m:mn>2 </m:mn><m:mo stretchy="false">(</m:mo><m:mn>–4</m:mn><m:mo stretchy="false">)</m:mo><m:mo>+</m:mo><m:mn>3</m:mn><m:mo stretchy="false">(</m:mo><m:mn>2 </m:mn><m:mo stretchy="false">)</m:mo>
          </m:mrow>
         </m:mtd>
         <m:mtd columnalign="right">
          <m:mrow>
           <m:mspace width="0.5em"/><m:mspace width="0.5em"/><m:mn>−1</m:mn><m:mo stretchy="false">(</m:mo><m:mn>–1</m:mn><m:mo stretchy="false">)</m:mo><m:mo>+</m:mo><m:mn>2 </m:mn><m:mo stretchy="false">(</m:mo><m:mn>0</m:mn><m:mo stretchy="false">)</m:mo><m:mo>+</m:mo><m:mn>3</m:mn><m:mo stretchy="false">(</m:mo><m:mn>3</m:mn><m:mo stretchy="false">)</m:mo>
          </m:mrow>
         </m:mtd>
        </m:mtr>
        <m:mtr columnalign="right">
         <m:mtd columnalign="right">
          <m:mrow>
           <m:mn>4</m:mn><m:mo stretchy="false">(</m:mo><m:mn>5</m:mn><m:mo stretchy="false">)</m:mo><m:mo>+</m:mo><m:mn>0</m:mn><m:mo stretchy="false">(</m:mo><m:mn>–4</m:mn><m:mo stretchy="false">)</m:mo><m:mo>+</m:mo><m:mn>5</m:mn><m:mo stretchy="false">(</m:mo><m:mn>2 </m:mn><m:mo stretchy="false">)</m:mo>
          </m:mrow>
         </m:mtd>
         <m:mtd columnalign="right">
          <m:mrow>

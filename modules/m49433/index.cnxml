    </m:mtr>
    <m:mtr>
     <m:mtd>
      <m:mi>c</m:mi>
     </m:mtd>
     <m:mtd>
      <m:mi>d</m:mi>
     </m:mtd>
    </m:mtr>

   </m:mtable>
  </m:mrow> <m:mo>]</m:mo></m:mrow><m:mspace width="0.5em"/><m:mtext>  y  </m:mtext><m:mi>B</m:mi><m:mo>=</m:mo><m:mrow><m:mo>[</m:mo> <m:mrow>
   <m:mtable>
    <m:mtr>
     <m:mtd>
      <m:mi>e</m:mi>
     </m:mtd>
     <m:mtd>
      <m:mi>f</m:mi>
     </m:mtd>
    </m:mtr>
    <m:mtr>
     <m:mtd>
      <m:mi>g</m:mi>
     </m:mtd>
     <m:mtd>
      <m:mi>h</m:mi>
     </m:mtd>
    </m:mtr>

   </m:mtable>
  </m:mrow> <m:mo>]</m:mo></m:mrow>
 </m:mrow>
</m:math>
</equation></problem>
  <solution id="fs-id1165131851993">


  <para id="fs-id1165137569645">Sume las entradas correspondientes.</para>
    <equation id="fs-id1165137862652" class="unnumbered"><label/> <m:math display="block">
 <m:mrow>
  <m:mtable columnalign="left">
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mi>A</m:mi><m:mo>+</m:mo><m:mi>B</m:mi><m:mo>=</m:mo><m:mrow><m:mo>[</m:mo> <m:mrow>
       <m:mtable>
        <m:mtr>
         <m:mtd>
          <m:mi>a</m:mi>
         </m:mtd>
         <m:mtd>
          <m:mi>b</m:mi>

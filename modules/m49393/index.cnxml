</m:msup>
<m:mi>x</m:mi>
</m:mrow>
</m:mfrac>
<m:mo>=</m:mo><m:msup>
<m:mrow>
<m:mi>sec</m:mi>
</m:mrow>
<m:mn>2</m:mn>
</m:msup>
<m:mi>x</m:mi><m:mo>+</m:mo><m:msup>
<m:mrow>

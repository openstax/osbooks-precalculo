<equation id="Equation_07_01_11" class="unnumbered"><label/> <m:math>
<m:mrow>
<m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>θ</m:mi><m:mo>=</m:mo><m:mfrac>
<m:mn>1</m:mn>
<m:mrow>
<m:mi>sec</m:mi><m:mspace width="0.3em"/><m:mi>θ</m:mi>
</m:mrow>
</m:mfrac>

</m:mrow>
</m:math>
</equation>
<equation id="Equation_07_01_12" class="unnumbered"><label/> <m:math display="block">
<m:mrow>
<m:mi>tan</m:mi><m:mspace width="0.3em"/><m:mi>θ</m:mi><m:mo>=</m:mo><m:mfrac>
<m:mn>1</m:mn>
<m:mrow>

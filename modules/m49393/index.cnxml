      <m:mi>cos</m:mi>
     </m:mrow>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mi>x</m:mi>
   </m:mrow>
  </m:mfrac>
  <m:mo>+</m:mo><m:mfrac>
   <m:mrow>
    <m:msup>
     <m:mrow>
      <m:mi>sen</m:mi>
     </m:mrow>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mi>x</m:mi>
   </m:mrow>
   <m:mrow>
    <m:msup>
     <m:mrow>
      <m:mi>cos</m:mi>
     </m:mrow>
     <m:mn>2</m:mn>
    </m:msup>
    <m:mi>x</m:mi>
   </m:mrow>
  </m:mfrac>
  <m:mo>=</m:mo><m:mn>1</m:mn><m:mo>+</m:mo><m:mn>2</m:mn><m:mspace width="0.5em"/><m:msup>
   <m:mrow>
    <m:mi>tan</m:mi>
   </m:mrow>
   <m:mn>2</m:mn>
  </m:msup>
  <m:mi>x</m:mi>
 </m:mrow>
</m:math> </para></problem>
<solution id="fs-id2558764">
<para id="fs-id2558767"> Las respuestas variarán. Una prueba de ejemplo:<newline/> <m:math>

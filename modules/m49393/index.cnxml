<m:mi>cos</m:mi>
<m:mn>3</m:mn>
</m:msup>
<m:mi>x</m:mi><m:mo>=</m:mo><m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi><m:mrow><m:mo>(</m:mo>
<m:mrow>
<m:mn>1</m:mn><m:mo>-</m:mo><m:msup>
<m:mrow>
<m:mi>cos</m:mi>
</m:mrow>
<m:mn>2</m:mn>
</m:msup>
<m:mi>x</m:mi>
</m:mrow>
<m:mo>)</m:mo></m:mrow></m:mrow></m:math> <newline/> <m:math><m:mrow><m:mo>=</m:mo><m:mi>cos</m:mi><m:mi>x</m:mi><m:msup>
<m:mi>sen</m:mi>
<m:mn>2</m:mn>
</m:msup>
<m:mi>x</m:mi></m:mrow>
</m:math>
</para></solution></exercise><exercise id="fs-id2955162" class="material-set-2"><problem id="fs-id2955163"><para id="fs-id2955164"> <m:math>
<m:mrow>
<m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi><m:mrow><m:mo>(</m:mo>
<m:mrow>
<m:mi>tan</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi><m:mo>−</m:mo><m:mi>sec</m:mi><m:mrow><m:mo>(</m:mo>
<m:mrow>
<m:mo>-</m:mo><m:mi>x</m:mi>
</m:mrow>
<m:mo>)</m:mo></m:mrow>
</m:mrow>
<m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mi>sen</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi><m:mo>–</m:mo><m:mn>1</m:mn>
</m:mrow>
</m:math>
</para></problem></exercise>
<exercise id="fs-id2955223" class="material-set-2"><problem id="fs-id2955224"><para id="fs-id2955225"> <m:math>

<row>
<entry align="center">Identidades recíprocas</entry>
<entry align="center"> <m:math>
<m:mtable columnalign="left">
<m:mtr>
<m:mtd>
<m:mi>sen</m:mi><m:mspace width="0.3em"/><m:mi>θ</m:mi><m:mo>=</m:mo><m:mfrac>
<m:mn>1</m:mn>
<m:mrow>
<m:mi>csc</m:mi><m:mspace width="0.3em"/><m:mi>θ</m:mi>
</m:mrow>
</m:mfrac>

</m:mtd>
</m:mtr>
<m:mtr>
<m:mtd>
<m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>θ</m:mi><m:mo>=</m:mo><m:mfrac>
<m:mn>1</m:mn>
<m:mrow>
<m:mi>sec</m:mi><m:mspace width="0.3em"/><m:mi>θ</m:mi>
</m:mrow>
</m:mfrac>


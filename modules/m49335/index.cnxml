  <m:mfrac>
   <m:mrow>
    <m:mn>2 </m:mn><m:mo>-</m:mo><m:mn>3</m:mn><m:mi>i</m:mi>
   </m:mrow>
   <m:mrow>
    <m:mn>4</m:mn><m:mo>+</m:mo><m:mn>3</m:mn><m:mi>i</m:mi>
   </m:mrow>
  </m:mfrac>

 </m:mrow>
</m:math> </para></problem>
</exercise>
<exercise id="fs-id1165134422291">
<problem id="fs-id1165134422293"><para id="fs-id1165134422294"> <m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:mn>3</m:mn><m:mo>+</m:mo><m:mn>4</m:mn><m:mi>i</m:mi>
   </m:mrow>
   <m:mrow>
    <m:mn>2 </m:mn><m:mo>-</m:mo><m:mi>i</m:mi>
   </m:mrow>
  </m:mfrac>

 </m:mrow>
</m:math> </para></problem>
<solution id="fs-id1165135530609"><para id="fs-id1165135530610"> <m:math>
 <m:mrow>
  <m:mfrac>
   <m:mn>2 </m:mn>
   <m:mn>5</m:mn>
  </m:mfrac>
  <m:mo>+</m:mo><m:mfrac>
   <m:mrow>
    <m:mn>11</m:mn>
   </m:mrow>
   <m:mn>5</m:mn>
  </m:mfrac>
  <m:mi>i</m:mi>
 </m:mrow>
</m:math> </para></solution>
</exercise>
<exercise id="fs-id1165135191481">
<problem id="fs-id1165135191484"><para id="fs-id1165135191485"> <m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:mn>2 </m:mn><m:mo>+</m:mo><m:mn>3</m:mn><m:mi>i</m:mi>
   </m:mrow>
   <m:mrow>
    <m:mn>2 </m:mn><m:mo>-</m:mo><m:mn>3</m:mn><m:mi>i</m:mi>
   </m:mrow>
  </m:mfrac>

 </m:mrow>
</m:math> </para></problem>
</exercise>
<exercise id="fs-id1165135481302">
<problem id="fs-id1165135481304"><para id="fs-id1165135481306"> <m:math>
 <m:mrow>
  <m:msqrt>
   <m:mrow>
    <m:mo>-</m:mo><m:mn>9</m:mn>
   </m:mrow>
  </m:msqrt>
  <m:mo>+</m:mo><m:mn>3</m:mn><m:msqrt>
   <m:mrow>
    <m:mo>-</m:mo><m:mn>16</m:mn>
   </m:mrow>
  </m:msqrt>

 </m:mrow>
</m:math> </para></problem>
<solution id="fs-id1165135596469"><para id="fs-id1165135596470"> <m:math>
 <m:mrow>
  <m:mn>15</m:mn><m:mi>i</m:mi>
 </m:mrow>
</m:math> </para></solution>
</exercise>
<exercise id="fs-id1165135683510">
<problem id="fs-id1165135683512"><para id="fs-id1165135683514"> <m:math>
 <m:mrow>
  <m:mo>-</m:mo><m:msqrt>
   <m:mrow>
    <m:mo>-</m:mo><m:mn>4</m:mn>
   </m:mrow>
  </m:msqrt>
  <m:mo>-</m:mo><m:mn>4</m:mn><m:msqrt>
   <m:mrow>
    <m:mo>-</m:mo><m:mn>25</m:mn>
   </m:mrow>
  </m:msqrt>

 </m:mrow>
</m:math> </para></problem>
</exercise>
<exercise id="fs-id1165135506341">
<problem id="fs-id1165135506343"><para id="fs-id1165135506344"> <m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:mn>2 </m:mn><m:mo>+</m:mo><m:msqrt>
     <m:mrow>
      <m:mo>-</m:mo><m:mn>12</m:mn>
     </m:mrow>
    </m:msqrt>

   </m:mrow>

          <m:mn>0</m:mn>
         </m:mtd>
        </m:mtr>
        <m:mtr>
         <m:mtd>
          <m:mn>0</m:mn>
         </m:mtd>
         <m:mtd>
          <m:mn>1</m:mn>
         </m:mtd>
        </m:mtr>

       </m:mtable>
      </m:mrow> <m:mo>]</m:mo></m:mrow><m:mspace width="0.5em"/><m:mtext>  </m:mtext><m:mrow><m:mo>[</m:mo> <m:mrow>
       <m:mtable>
        <m:mtr>
         <m:mtd>
          <m:mi>x</m:mi>
         </m:mtd>
        </m:mtr>
        <m:mtr>
         <m:mtd>
          <m:mi>y</m:mi>
         </m:mtd>
        </m:mtr>

       </m:mtable>
      </m:mrow> <m:mo>]</m:mo></m:mrow><m:mo>=</m:mo><m:mrow><m:mo>[</m:mo> <m:mrow>
       <m:mtable columnalign="right">
        <m:mtr columnalign="right">
         <m:mtd columnalign="right">
          <m:mrow>
           <m:mn>11</m:mn><m:mo stretchy="false">(</m:mo><m:mn>5</m:mn><m:mo stretchy="false">)</m:mo><m:mo>+</m:mo><m:mo stretchy="false">(</m:mo><m:mn>−8</m:mn><m:mo stretchy="false">)</m:mo><m:mn>7</m:mn>
          </m:mrow>
         </m:mtd>
        </m:mtr>
        <m:mtr columnalign="right">
         <m:mtd columnalign="right">
          <m:mrow>
           <m:mn>-4</m:mn><m:mo stretchy="false">(</m:mo><m:mn>5</m:mn><m:mo stretchy="false">)</m:mo><m:mo>+</m:mo><m:mn>3</m:mn><m:mo stretchy="false">(</m:mo><m:mn>7</m:mn><m:mo stretchy="false">)</m:mo>
          </m:mrow>
         </m:mtd>
        </m:mtr>

       </m:mtable>
      </m:mrow> <m:mo>]</m:mo></m:mrow>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mspace width="0.5em"/><m:mrow><m:mo>[</m:mo> <m:mrow>
       <m:mtable>
        <m:mtr>

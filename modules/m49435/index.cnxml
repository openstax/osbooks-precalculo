         </m:mtd>
         <m:mtd columnalign="right">
          <m:mrow>
           <m:mspace width="0.5em"/><m:mn>−1</m:mn><m:mo stretchy="false">(</m:mo><m:mn>1</m:mn><m:mo stretchy="false">)</m:mo><m:mo>+</m:mo><m:mn>0</m:mn><m:mo stretchy="false">(</m:mo><m:mn>1</m:mn><m:mo stretchy="false">)</m:mo><m:mo>+</m:mo><m:mn>1</m:mn><m:mo stretchy="false">(</m:mo><m:mn>1</m:mn><m:mo stretchy="false">)</m:mo>
          </m:mrow>
         </m:mtd>
        </m:mtr>
        <m:mtr columnalign="right">
         <m:mtd columnalign="right">
          <m:mrow>
           <m:mn>6</m:mn><m:mo stretchy="false">(</m:mo><m:mn>2 </m:mn><m:mo stretchy="false">)</m:mo><m:mo>+</m:mo><m:mn>−2</m:mn><m:mo stretchy="false">(</m:mo><m:mn>3</m:mn><m:mo stretchy="false">)</m:mo><m:mo>+</m:mo><m:mn>−3</m:mn><m:mo stretchy="false">(</m:mo><m:mn>2 </m:mn><m:mo stretchy="false">)</m:mo>
          </m:mrow>
         </m:mtd>
         <m:mtd columnalign="right">
          <m:mrow>
           <m:mspace width="0.5em"/><m:mn>6</m:mn><m:mo stretchy="false">(</m:mo><m:mn>3</m:mn><m:mo stretchy="false">)</m:mo><m:mo>+</m:mo><m:mn>−2</m:mn><m:mo stretchy="false">(</m:mo><m:mn>3</m:mn><m:mo stretchy="false">)</m:mo><m:mo>+</m:mo><m:mn>−3</m:mn><m:mo stretchy="false">(</m:mo><m:mn>4</m:mn><m:mo stretchy="false">)</m:mo>
          </m:mrow>
         </m:mtd>
         <m:mtd columnalign="right">
          <m:mrow>
           <m:mspace width="0.5em"/><m:mn>6</m:mn><m:mo stretchy="false">(</m:mo><m:mn>1</m:mn><m:mo stretchy="false">)</m:mo><m:mo>+</m:mo><m:mn>−2</m:mn><m:mo stretchy="false">(</m:mo><m:mn>1</m:mn><m:mo stretchy="false">)</m:mo><m:mo>+</m:mo><m:mn>−3</m:mn><m:mo stretchy="false">(</m:mo><m:mn>1</m:mn><m:mo stretchy="false">)</m:mo>
          </m:mrow>
         </m:mtd>
        </m:mtr>

       </m:mtable>
      </m:mrow> <m:mo>]</m:mo></m:mrow>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mspace width="0.5em"/><m:mo>=</m:mo><m:mrow><m:mo>[</m:mo> <m:mrow>
       <m:mtable columnalign="right">
        <m:mtr columnalign="right">
         <m:mtd columnalign="right">
          <m:mn>1</m:mn>
         </m:mtd>
         <m:mtd columnalign="right">
          <m:mn>0</m:mn>
         </m:mtd>
         <m:mtd columnalign="right">
          <m:mn>0</m:mn>
         </m:mtd>
        </m:mtr>
        <m:mtr columnalign="right">
         <m:mtd columnalign="right">
          <m:mn>0</m:mn>
         </m:mtd>
         <m:mtd columnalign="right">
          <m:mn>1</m:mn>
         </m:mtd>
         <m:mtd columnalign="right">
          <m:mn>0</m:mn>
         </m:mtd>
        </m:mtr>
        <m:mtr columnalign="right">
         <m:mtd columnalign="right">
          <m:mn>0</m:mn>
         </m:mtd>
         <m:mtd columnalign="right">
          <m:mn>0</m:mn>
         </m:mtd>
         <m:mtd columnalign="right">
          <m:mn>1</m:mn>
         </m:mtd>
        </m:mtr>

       </m:mtable>
      </m:mrow> <m:mo>]</m:mo></m:mrow>
     </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable>
 </m:mrow>
</m:math>
</equation></commentary>
        </exercise></example>

      <note id="fs-id1165137446254" class="precalculus try">
        <label>Ejercicio</label>
        <exercise id="ti_09_07_03">
          <problem id="fs-id1165135536370"><para id="fs-id1165135536371">Halle la inversa de la matriz <m:math>
            <m:mrow>
              <m:mn>3</m:mn><m:mo>×</m:mo><m:mn>3</m:mn>
            </m:mrow>
          </m:math>.</para>
            <equation id="fs-id1165137732890" class="unnumbered"><label/> <m:math display="block">
              <m:mrow>
                <m:mi>A</m:mi><m:mo>=</m:mo><m:mrow><m:mo>[</m:mo> <m:mrow>

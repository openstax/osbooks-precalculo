    <exercise id="fs-id1165137455061"><problem id="fs-id1165135186823"><para id="fs-id1165135186824"> <m:math>
 <m:mrow>
  <m:msub>
   <m:mi>r</m:mi>
   <m:mn>1</m:mn>
  </m:msub>
  <m:msup>
   <m:mrow/>
   <m:mn>2 </m:mn>
  </m:msup>
  <m:mo>=</m:mo><m:mi>sen</m:mi><m:mspace width="0.3em"/><m:mi>θ</m:mi><m:mo>,</m:mo><m:msub>
   <m:mi>r</m:mi>
   <m:mn>2</m:mn>
  </m:msub>
  <m:msup>
   <m:mrow/>
   <m:mn>2</m:mn>
  </m:msup>

      </m:msub>
      <m:mo>=</m:mo><m:mn>2 </m:mn><m:mo>+</m:mo><m:mn>2</m:mn><m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>θ</m:mi>

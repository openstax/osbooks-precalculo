                  </m:math>
                  </entry>
                  <entry align="center"> <m:math>
                    <m:mrow>
                      <m:mi>x</m:mi><m:mo>=</m:mo><m:mn>2</m:mn><m:mi>cos</m:mi><m:mrow><m:mo>(</m:mo>
                        <m:mrow>
                          <m:mfrac>
                            <m:mi>π</m:mi>
                            <m:mn>6</m:mn>
                          </m:mfrac>


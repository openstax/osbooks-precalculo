      <m:mo stretchy="false">(</m:mo><m:mn>3</m:mn><m:mo>,</m:mo><m:mspace width="0.5em"/><m:mo>−</m:mo><m:mn>50</m:mn><m:mo stretchy="false">)</m:mo></m:mrow>

<m:mi>x</m:mi>
<m:mn>3</m:mn>
</m:msup>
<m:mo>-</m:mo><m:msup>
<m:mi>x</m:mi>
<m:mn>2 </m:mn>
</m:msup>
<m:mo>-</m:mo><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>1</m:mn>
</m:mtd>
</m:mtr>
<m:mtr>
<m:mtd>
<m:mrow><m:mo>(</m:mo>
<m:mrow>
<m:mi>f</m:mi><m:mo>- −</m:mo><m:mi>g</m:mi></m:mrow>
<m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo>
<m:mi>x</m:mi>
<m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mi>f</m:mi><m:mrow><m:mo>(</m:mo>
<m:mi>x</m:mi>
<m:mo>)</m:mo></m:mrow><m:mo>-</m:mo><m:mi>g</m:mi><m:mrow><m:mo>(</m:mo>
<m:mi>x</m:mi>
<m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mrow><m:mo>(</m:mo>
<m:mrow>
<m:mi>x</m:mi><m:mo>–</m:mo><m:mn>1</m:mn></m:mrow>

                      </m:mfrac>

                    </m:mrow>
                    <m:mrow>
                      <m:mn>1</m:mn><m:mo>+</m:mo><m:mfrac>
                        <m:mrow>
                          <m:msup>
                            <m:mrow>
                              <m:mi>sen</m:mi>
                            </m:mrow>
                            <m:mn>2</m:mn>
                          </m:msup>
                          <m:mi>x</m:mi>
                        </m:mrow>
                        <m:mrow>
                          <m:msup>
                            <m:mrow>
                              <m:mi>cos</m:mi>
                            </m:mrow>
                            <m:mn>2</m:mn>
                          </m:msup>
                          <m:mi>x</m:mi>
                        </m:mrow>
                      </m:mfrac>

                    </m:mrow>
                  </m:mfrac>
                  <m:mo>=</m:mo><m:mfrac>
                    <m:mrow>
                      <m:mfrac>
                        <m:mrow>
                          <m:mn>2</m:mn><m:mi>sen</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi>
                        </m:mrow>
                        <m:mrow>
                          <m:mi>cos</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi>
                        </m:mrow>
                      </m:mfrac>

                    </m:mrow>
                    <m:mrow>
                      <m:mfrac>
                        <m:mrow>
                          <m:msup>

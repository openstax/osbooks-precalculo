                        </m:mrow>
                        <m:mn>2</m:mn>
                      </m:msup>
                      <m:mi>x</m:mi>
                    </m:mrow>
                  </m:mfrac>
                  <m:mo>=</m:mo><m:mfrac>
                    <m:mrow>
                      <m:mfrac>
                        <m:mrow>

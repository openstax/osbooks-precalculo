                            </m:mrow>
                            <m:mn>2</m:mn>
                          </m:msup>
                          <m:mi>x</m:mi><m:mo>+</m:mo><m:msup>
                            <m:mrow>
                              <m:mi>sen</m:mi>
                            </m:mrow>
                            <m:mn>2</m:mn>
                          </m:msup>
                          <m:mi>x</m:mi>
                        </m:mrow>
                        <m:mrow>
                          <m:msup>
                            <m:mrow>
                              <m:mi>cos</m:mi>
                            </m:mrow>
                            <m:mn>2</m:mn>
                          </m:msup>
                          <m:mi>x</m:mi>
                        </m:mrow>
                      </m:mfrac>

                    </m:mrow>
                  </m:mfrac>
                  <m:mo>=</m:mo></m:mrow></m:math> <newline/> <m:math><m:mrow>
                  <m:mfrac>
                    <m:mrow>
                      <m:mn>2</m:mn><m:mi>sen</m:mi><m:mspace width="0.2em"/><m:mi>x</m:mi>
                    </m:mrow>
                    <m:mrow>
                      <m:mi>cos</m:mi><m:mspace width="0.2em"/><m:mi>x</m:mi>
                    </m:mrow>
                  </m:mfrac>
                  <m:mo>.</m:mo><m:mfrac>
                    <m:mrow>
                      <m:msup>

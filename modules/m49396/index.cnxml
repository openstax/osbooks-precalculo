                      <m:mi>x</m:mi>
                    </m:mrow>
                    <m:mn>1</m:mn>
                  </m:mfrac>
                  <m:mo>=</m:mo><m:mn>2</m:mn><m:mi>sen</m:mi><m:mi>x</m:mi><m:mi>cos</m:mi><m:mspace width="0.2em"/><m:mi>x</m:mi><m:mo>=</m:mo><m:mi>sen</m:mi><m:mo stretchy="false">(</m:mo><m:mn>2 </m:mn><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo></m:mrow>

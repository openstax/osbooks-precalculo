            </m:mrow>
            <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mi>cos</m:mi><m:mo stretchy="false">(</m:mo><m:mn>2</m:mn><m:mi>θ</m:mi><m:mo stretchy="false">)</m:mo><m:mo>,</m:mo>
        </m:mrow>
      </m:math> y <m:math>
        <m:mrow>
          <m:mi>tan</m:mi><m:mo stretchy="false">(</m:mo><m:mn>2</m:mn><m:mi>θ</m:mi><m:mo stretchy="false">).</m:mo>
        </m:mrow>

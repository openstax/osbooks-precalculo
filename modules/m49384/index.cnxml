</media>
</figure>

<exercise id="fs-id1165135424671"><problem id="fs-id1165135424672"><para id="fs-id1165135424673"> <m:math>
 <m:mrow>
  <m:mi>sen</m:mi><m:mspace width="0.3em"/><m:mi>A</m:mi>
 </m:mrow>
</m:math>
 </para></problem>
<solution id="fs-id1165137734516"><para id="fs-id1165134122790"> <m:math>
 <m:mrow>
  <m:mfrac>
   <m:mrow>
    <m:mn>11</m:mn><m:msqrt>
     <m:mrow>
      <m:mn>157</m:mn>
     </m:mrow>
    </m:msqrt>

   </m:mrow>
   <m:mrow>
    <m:mn>157</m:mn>
   </m:mrow>
  </m:mfrac>

 </m:mrow>
</m:math>
</para></solution></exercise>

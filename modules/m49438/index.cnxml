    <exercise id="fs-id1339864"><problem id="fs-id1339865"><para id="fs-id1339866"> <m:math>
     <m:mrow>
      <m:mn>4</m:mn><m:msup>
       <m:mi>x</m:mi>
       <m:mn>2 </m:mn>
      </m:msup>

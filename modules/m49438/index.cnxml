     </m:mrow>
    </m:math>
    </para></problem></exercise>
    <exercise id="fs-id1351300"><problem id="fs-id1351301"><para id="fs-id1351302"> <m:math>
     <m:mrow>
      <m:mn>4</m:mn><m:msup>
       <m:mi>x</m:mi>
       <m:mn>2 </m:mn>
      </m:msup>
      <m:mo>+</m:mo><m:mn>9</m:mn><m:msup>
       <m:mi>y</m:mi>
       <m:mn>2 </m:mn>
      </m:msup>
      <m:mo>=</m:mo><m:mn>36</m:mn>
     </m:mrow>
    </m:math>
    </para></problem>
     <solution id="fs-id1194848"><para id="fs-id1194849">sí; <m:math>
      <m:mrow>
       <m:mfrac>
        <m:mrow>
         <m:msup>
          <m:mi>x</m:mi>
          <m:mn>2</m:mn>
         </m:msup>

        </m:mrow>
        <m:mrow>
         <m:msup>
          <m:mn>3</m:mn>
          <m:mn>2 </m:mn>
         </m:msup>

        </m:mrow>
       </m:mfrac>
       <m:mo>+</m:mo><m:mfrac>
        <m:mrow>
         <m:msup>
          <m:mi>y</m:mi>
          <m:mn>2</m:mn>
         </m:msup>

        </m:mrow>
        <m:mrow>
         <m:msup>
          <m:mn>2 </m:mn>
          <m:mn>2</m:mn>
         </m:msup>

        </m:mrow>
       </m:mfrac>
       <m:mo>=</m:mo><m:mn>1</m:mn>
      </m:mrow>
     </m:math>
     </para></solution></exercise>
    <exercise id="fs-id1339824"><problem id="fs-id1339826"><para id="fs-id1339827"> <m:math>

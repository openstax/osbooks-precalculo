            <m:mo>)</m:mo></m:mrow>
          </m:mrow>
          <m:mn>2 </m:mn>
         </m:msup>

        </m:mrow>
       </m:mfrac>
       <m:mo>=</m:mo><m:mn>1</m:mn>
      </m:mrow>

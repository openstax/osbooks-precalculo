        </m:mtd>
       </m:mtr>

      </m:mtable><m:mtext>Obtenga la raíz cuadrada de ambos lados</m:mtext><m:mo>.</m:mo>
     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mi>c</m:mi><m:mo>=</m:mo><m:mo>±</m:mo><m:msqrt>
       <m:mrow>
        <m:mn>1775 </m:mn>
       </m:mrow>
      </m:msqrt>

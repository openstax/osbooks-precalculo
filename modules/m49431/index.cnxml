             <m:mn>1</m:mn>
             <m:mn>2 </m:mn>
            </m:mfrac>
            <m:mrow><m:mo>(</m:mo>
             <m:mrow>
              <m:msqrt>
               <m:mrow>
                <m:mn>73</m:mn>
               </m:mrow>
              </m:msqrt>
              <m:mn>−5</m:mn>
             </m:mrow>
             <m:mo>)</m:mo></m:mrow>
           </m:mrow>
          </m:msqrt>
          <m:mo>,</m:mo><m:mfrac>

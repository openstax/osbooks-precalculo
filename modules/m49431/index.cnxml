           </m:mrow>
           <m:mrow>
            <m:mn>29</m:mn>
           </m:mrow>
          </m:mfrac>

         </m:mrow>
        </m:msqrt>

       </m:mrow>
       <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>

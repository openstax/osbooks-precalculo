          </m:mfrac>

         </m:mrow>
         <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
          <m:mrow>
           <m:mfrac>
            <m:mrow>
             <m:msqrt>
              <m:mn>2</m:mn>
             </m:msqrt>

            </m:mrow>
            <m:mn>2</m:mn>
           </m:mfrac>
           <m:mo>,</m:mo><m:mo>-</m:mo><m:mfrac>
            <m:mrow>

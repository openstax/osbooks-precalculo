          <m:mrow>
           <m:msqrt>
            <m:mrow>
             <m:mn>398</m:mn>
            </m:mrow>
           </m:msqrt>

          </m:mrow>
          <m:mn>4</m:mn>
         </m:mfrac>
         <m:mo>,</m:mo><m:mfrac>
          <m:mrow>
           <m:mn>199</m:mn>
          </m:mrow>
          <m:mn>4</m:mn>
         </m:mfrac>

        </m:mrow>
        <m:mo>)</m:mo></m:mrow><m:mo>,</m:mo><m:mrow><m:mo>(</m:mo>
         <m:mrow>
          <m:mfrac>
           <m:mrow>
            <m:msqrt>
             <m:mrow>

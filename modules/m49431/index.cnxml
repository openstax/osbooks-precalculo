    <para id="fs-id1165134298422"> <m:math>
     <m:mrow>
      <m:mrow><m:mo>(</m:mo>
       <m:mrow>
        <m:mn>−2</m:mn><m:msqrt>
         <m:mrow>
          <m:mfrac>

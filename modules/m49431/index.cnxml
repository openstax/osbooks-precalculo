           <m:mrow>
            <m:mn>199</m:mn>
           </m:mrow>
           <m:mn>4</m:mn>
          </m:mfrac>

         </m:mrow>
         <m:mo>)</m:mo></m:mrow>
      </m:mrow>
     </m:math>
     </para>
    </solution></exercise>

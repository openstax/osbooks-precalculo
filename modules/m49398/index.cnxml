       <m:mn>2 </m:mn>
      </m:msup>
      <m:mo>-</m:mo><m:msup>
       <m:mi>a</m:mi>
       <m:mn>2</m:mn>
      </m:msup>

     </m:mrow>
    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>

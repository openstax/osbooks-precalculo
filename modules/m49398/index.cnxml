  </m:msup>
  <m:mi>x</m:mi><m:mo>-</m:mo><m:mi>sen</m:mi><m:mspace width="0.3em"/><m:mi>x</m:mi><m:mo>=</m:mo><m:mn>0</m:mn>
 </m:mrow>
</m:math>

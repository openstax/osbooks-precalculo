<exercise id="fs-id1165137694067">
<problem id="fs-id1165137694069">
<para id="fs-id1165137806803"> <m:math>
 <m:mrow>
  <m:mtable>
   <m:mtr>
    <m:mtd>
     <m:mrow>
      <m:msub>
       <m:mi>a</m:mi>
       <m:mn>6</m:mn>
      </m:msub>
      <m:mo>=</m:mo><m:mn>25</m:mn><m:mo>,</m:mo>
     </m:mrow>
    </m:mtd>
    <m:mtd>
     <m:mrow>

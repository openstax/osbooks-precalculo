   <m:mi>n</m:mi>
  </m:msub>
  <m:mo>=</m:mo><m:mn>12</m:mn><m:mo>⋅</m:mo><m:msup>
   <m:mrow>
    <m:mrow><m:mo>(</m:mo>
     <m:mrow>
      <m:mo>-</m:mo><m:mfrac>
       <m:mn>1</m:mn>
       <m:mn>2 </m:mn>
      </m:mfrac>

     </m:mrow>
    <m:mo>)</m:mo></m:mrow>
   </m:mrow>
   <m:mrow>
    <m:mi>n</m:mi><m:mo>–</m:mo><m:mn>1</m:mn>
   </m:mrow>
  </m:msup>

 </m:mrow>
</m:math> </para></problem>
<solution id="fs-id1165135161218">
<para id="fs-id1165137653407"> <m:math>
 <m:mrow>
  <m:mn>12</m:mn><m:mo>,</m:mo><m:mo>- −</m:mo><m:mn>6</m:mn><m:mo>,</m:mo><m:mn>3</m:mn><m:mo>,</m:mo><m:mo>-</m:mo><m:mfrac>
   <m:mn>3</m:mn>
   <m:mn>2 </m:mn>
  </m:mfrac>
  <m:mo>,</m:mo><m:mfrac>
   <m:mn>3</m:mn>
   <m:mn>4</m:mn>
  </m:mfrac>

 </m:mrow>
</m:math> </para></solution>
</exercise>


      </m:mrow>
      <m:mn>2</m:mn>
     </m:msup>
     <m:mi>t</m:mi><m:mo>=</m:mo><m:mn>3</m:mn>
    </m:mrow>
   </m:math>
   </para></problem>
   <solution id="fs-id2738088">
    <para id="fs-id2738090"> <m:math>
     <m:mrow>
      <m:msup>
       <m:mrow>
        <m:mi>sen</m:mi>
       </m:mrow>
       <m:mrow>
        <m:mo>−</m:mo><m:mn>1</m:mn>
       </m:mrow>
      </m:msup>
      <m:mrow><m:mo>(</m:mo>
       <m:mrow>

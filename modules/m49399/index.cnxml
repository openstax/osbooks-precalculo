     <row>
      <entry align="center">Forma típica de ecuación sinusoidal</entry>
      <entry align="center"> <m:math>
       <m:mrow>
        <m:mi>y</m:mi><m:mo>=</m:mo><m:mi>A</m:mi><m:mspace width="0.3em"/><m:mi>sen</m:mi><m:mrow><m:mo>(</m:mo>
         <m:mrow>

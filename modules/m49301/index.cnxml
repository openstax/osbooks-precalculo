   </m:math>
   </para>
   <list id="fs-id1165135320063" list-type="enumerated" number-style="arabic" class="circled">
    <item><span class="token">Ⓐ</span> Evalúe <m:math>
     <m:mrow>
      <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mn>5</m:mn><m:mo stretchy="false">)</m:mo><m:mo>.</m:mo></m:mrow>
    </m:math>
    </item>
    <item><span class="token">Ⓑ</span> Resuelva <m:math>
     <m:mrow>
      <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>4.</m:mn></m:mrow>
    </m:math>

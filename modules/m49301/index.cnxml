      <m:mn>2 </m:mn>
     </m:msup>
    <m:mo>-</m:mo><m:mn>7</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>3</m:mn></m:mrow>
  </m:math>
  </para>
 </problem>
</exercise>
<exercise id="fs-id1165137937596">
 <problem id="fs-id1165135181211">
  <para id="fs-id1165135181213"> <m:math>
   <m:mrow>
    <m:mi>f</m:mi><m:mrow><m:mo>(</m:mo>
     <m:mi>x</m:mi>
     <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>3</m:mn><m:mo>+</m:mo><m:msqrt>
      <m:mrow>
       <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>3</m:mn></m:mrow>
     </m:msqrt>
   </m:mrow>
  </m:math>
  </para>
 </problem>
 <solution id="fs-id1165137755505"><para id="fs-id1165137755506"><m:math>
 <m:mrow>
  <m:mtable>
   <m:mtr>
    <m:mtd>
     <m:mrow>
      <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mo>–</m:mo><m:mn>2 </m:mn><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>4</m:mn><m:mo>;</m:mo><m:mtext>  </m:mtext>
     </m:mrow>
    </m:mtd>
    <m:mtd>
     <m:mrow>
      <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mo>-</m:mo><m:mn>1</m:mn><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>4,414</m:mn><m:mo>;</m:mo>
     </m:mrow>
    </m:mtd>
    <m:mtd>
     <m:mrow>
      <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mn>0</m:mn><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>4,732</m:mn><m:mo>;</m:mo>
     </m:mrow>
    </m:mtd>
    <m:mtd>
     <m:mrow>
      <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mn>1</m:mn><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>5</m:mn><m:mo>;</m:mo>
     </m:mrow>
    </m:mtd>
    <m:mtd>
     <m:mrow>
      <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mn>2 </m:mn><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>5,236</m:mn>
     </m:mrow>
    </m:mtd>
   </m:mtr>

  </m:mtable>
 </m:mrow>
</m:math>
</para></solution></exercise>
<exercise id="fs-id1165134573828">
 <problem id="fs-id1165134573830">
  <para id="fs-id1165134573832"> <m:math>
   <m:mrow>
    <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mfrac>
     <m:mrow>
      <m:mi>x</m:mi><m:mo>-</m:mo><m:mn>2 </m:mn></m:mrow>
     <m:mrow>
      <m:mi>x</m:mi><m:mo>+</m:mo><m:mn>3</m:mn></m:mrow>
    </m:mfrac>
   </m:mrow>
  </m:math>
  </para>
 </problem>
</exercise>
<exercise id="fs-id1165133248574">
 <problem id="fs-id1165133248576">
  <para id="fs-id1165133248578"> <m:math>
   <m:mrow>
    <m:mi>f</m:mi><m:mrow><m:mo>(</m:mo>
     <m:mi>x</m:mi>
     <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:msup>
      <m:mn>3</m:mn>
      <m:mi>x</m:mi>
     </m:msup>
   </m:mrow>
  </m:math>
  </para>
 </problem>
 <solution id="fs-id1165137770304"><para id="fs-id1165137770305"> <m:math>
  <m:mrow>
   <m:mtable>
    <m:mtr>
     <m:mtd>
      <m:mrow>
       <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mo>–</m:mo><m:mn>2 </m:mn><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mfrac>
        <m:mn>1</m:mn>
        <m:mn>9</m:mn>
       </m:mfrac>
       <m:mo>;</m:mo></m:mrow>
     </m:mtd>
     <m:mtd>
      <m:mrow>
       <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mo>-</m:mo><m:mn>1</m:mn><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mfrac>
        <m:mn>1</m:mn>
        <m:mn>3</m:mn>
       </m:mfrac>
       <m:mo>;</m:mo></m:mrow>
     </m:mtd>
     <m:mtd>
      <m:mrow>
       <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mn>0</m:mn><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>1</m:mn><m:mo>;</m:mo></m:mrow>
     </m:mtd>
     <m:mtd>
      <m:mrow>
       <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mn>1</m:mn><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>3</m:mn><m:mo>;</m:mo></m:mrow>
     </m:mtd>
     <m:mtd>
      <m:mrow>
       <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mn>2 </m:mn><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>9</m:mn></m:mrow>
     </m:mtd>
    </m:mtr>

   </m:mtable></m:mrow>
 </m:math>
 </para></solution></exercise>
 <para id="fs-id1165135306461">En los siguientes ejercicios, evalúe las expresiones, dadas las funciones <m:math>
  <m:mrow>
   <m:mi>f</m:mi><m:mo>,</m:mo><m:mi>g</m:mi><m:mo>,</m:mo></m:mrow>
 </m:math> y <m:math>
  <m:mrow>
   <m:mi>h</m:mi><m:mo>:</m:mo></m:mrow>
 </m:math>
 </para><list id="eip-582"><item> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>3</m:mn><m:mi>x</m:mi><m:mo>-</m:mo><m:mn>2 </m:mn>
 </m:mrow>
</m:math>
</item>
<item> <m:math>
 <m:mrow>
  <m:mi>g</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>5</m:mn><m:mo>-</m:mo><m:msup>
   <m:mi>x</m:mi>
   <m:mn>2 </m:mn>
  </m:msup>

 </m:mrow>
</m:math>
</item>
<item> <m:math>
 <m:mrow>
  <m:mi>h</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mo>-</m:mo><m:mn>2 </m:mn><m:msup>
   <m:mi>x</m:mi>
   <m:mn>2 </m:mn>
  </m:msup>
  <m:mo>+</m:mo><m:mn>3</m:mn><m:mi>x</m:mi><m:mo>–</m:mo><m:mn>1</m:mn>
 </m:mrow>
</m:math>
</item>
</list>
<exercise id="fs-id1165135575197">

    <m:mrow>
     <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mo>–</m:mo><m:mn>2 </m:mn><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>14</m:mn><m:mo>;</m:mo></m:mrow>
   </m:math></item>
<item><span class="token">Ⓑ</span> <m:math>
    <m:mrow>
     <m:mi>x</m:mi><m:mo>=</m:mo><m:mn>3</m:mn></m:mrow>
   </m:math></item>
   </list>
  </solution>
 </exercise><exercise id="fs-id1165137935719"><problem id="fs-id1165135388490">
   <para id="fs-id1165135388492">Dada la función <m:math>
    <m:mrow>
     <m:mi>p</m:mi><m:mo stretchy="false">(</m:mo><m:mi>c</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:msup>

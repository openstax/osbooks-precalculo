  <para id="fs-id1165137812528"> <m:math>
   <m:mrow>
    <m:mi>f</m:mi><m:mrow><m:mo>(</m:mo>
     <m:mi>x</m:mi>
     <m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mn>8</m:mn><m:mo>-</m:mo><m:mn>3</m:mn><m:mi>x</m:mi></m:mrow>
  </m:math>

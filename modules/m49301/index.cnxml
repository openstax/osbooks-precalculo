      <m:mn>2 </m:mn>
      <m:mn>3</m:mn>
     </m:mfrac>
     <m:mi>t</m:mi><m:mo>;</m:mo></m:mrow>
   </m:math></item><item><span class="token">Ⓑ</span> <m:math>
    <m:mrow>
     <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mo>-</m:mo><m:mn>3</m:mn><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>8</m:mn><m:mo>;</m:mo></m:mrow>

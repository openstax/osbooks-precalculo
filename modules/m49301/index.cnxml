  <m:mrow>
   <m:mtable>
    <m:mtr>
     <m:mtd>
      <m:mrow>
       <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mo>–</m:mo><m:mn>2 </m:mn><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>14</m:mn><m:mo>;</m:mo></m:mrow>
     </m:mtd>
     <m:mtd>
      <m:mrow>
       <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mo>-</m:mo><m:mn>1</m:mn><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>11</m:mn><m:mo>;</m:mo></m:mrow>
     </m:mtd>
     <m:mtd>
      <m:mrow>
       <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mn>0</m:mn><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>8</m:mn><m:mo>;</m:mo></m:mrow>
     </m:mtd>
     <m:mtd>
      <m:mrow>
       <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mn>1</m:mn><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>5</m:mn><m:mo>;</m:mo></m:mrow>
     </m:mtd>
     <m:mtd>
      <m:mrow>
       <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mn>2 </m:mn><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>2 </m:mn></m:mrow>
     </m:mtd>
    </m:mtr>

   </m:mtable></m:mrow>
 </m:math>
 </para></solution></exercise>
<exercise id="fs-id1165135445749">
 <problem id="fs-id1165135445751">
  <para id="fs-id1165135445753"> <m:math>
   <m:mrow>
    <m:mi>f</m:mi><m:mrow><m:mo>(</m:mo>

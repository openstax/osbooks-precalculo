     <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>8</m:mn><m:mo>-</m:mo><m:mn>3</m:mn><m:mi>x</m:mi><m:mo>:</m:mo></m:mrow>
   </m:math>

<m:mo>-</m:mo><m:mn>2 </m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>5</m:mn><m:mi>y</m:mi><m:mo>=</m:mo><m:mn>20</m:mn>
</m:mrow>
</m:math>
</para>
</problem>
</exercise>

<exercise id="fs-id1165137415184">
<problem id="fs-id1165137415186">

<exercise id="fs-id1165135369390">
<problem id="fs-id1165135369392">
<para id="fs-id1165135369394"> <m:math>
<m:mrow>
<m:mtable>
<m:mtr>
<m:mtd>
<m:mrow>
<m:mn>6</m:mn><m:mi>x</m:mi><m:mo>-</m:mo><m:mn>9</m:mn><m:mi>y</m:mi><m:mo>=</m:mo><m:mn>10</m:mn>
</m:mrow>
</m:mtd>
</m:mtr>
<m:mtr>
<m:mtd>
<m:mrow>
<m:mn>3</m:mn><m:mi>x</m:mi><m:mo>+</m:mo><m:mn>2 </m:mn><m:mi>y</m:mi><m:mo>=</m:mo><m:mn>1</m:mn>
</m:mrow>
</m:mtd>
</m:mtr>

</m:mtable>
</m:mrow>
</m:math>
</para>
</problem>
<solution id="fs-id1165137436211">
<para id="fs-id1165135684913">perpendicular</para>
</solution>
</exercise>


<exercise id="fs-id1165135439831">
<problem id="fs-id1165135439833">
<para id="fs-id1165135439836"> <m:math><m:mrow><m:mtable><m:mtr><m:mtd> <m:mrow>  <m:mn>2 </m:mn><m:mi>x</m:mi><m:mo>-</m:mo><m:mn>3</m:mn><m:mi>y</m:mi><m:mo>=</m:mo><m:mn>12</m:mn> </m:mrow></m:mtd></m:mtr><m:mtr><m:mtd> <m:mrow>  <m:mn>5</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mi>x</m:mi><m:mo>=</m:mo><m:mn>30</m:mn> </m:mrow></m:mtd></m:mtr></m:mtable></m:mrow></m:math> </para>
</problem>
</exercise>

<exercise id="fs-id1165137416113">
<problem id="fs-id1165137806936">
<para id="fs-id1165137806939"> <m:math><m:mrow><m:mtable><m:mtr><m:mtd><m:mrow><m:mn>2 </m:mn><m:mi>x</m:mi><m:mo>=</m:mo><m:mi>y</m:mi><m:mo>-</m:mo><m:mn>3</m:mn></m:mrow></m:mtd></m:mtr><m:mtr><m:mtd><m:mrow><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>4</m:mn><m:mi>x</m:mi><m:mo>=</m:mo><m:mn>15</m:mn></m:mrow></m:mtd></m:mtr></m:mtable>
</m:mrow>
</m:math>
</para>
</problem>
<solution id="fs-id1165137476621">
<para id="fs-id1165137476624"> <m:math><m:mrow><m:mo stretchy="false">(</m:mo><m:mtext>2 </m:mtext><m:mo>,</m:mo><m:mtext> 7</m:mtext><m:mo stretchy="false">)</m:mo>
</m:mrow>
</m:math> grandes.
</para>
</solution>
</exercise>

<exercise id="fs-id1165137939656">
<problem id="fs-id1165137939658">
<para id="fs-id1165137939661"> <m:math>
<m:mrow>
<m:mtable>
<m:mtr>
<m:mtd>
<m:mrow>
<m:mi>x</m:mi><m:mo>-</m:mo><m:mn>2</m:mn><m:mi>y</m:mi><m:mo>+</m:mo><m:mn>2 </m:mn><m:mo>=</m:mo><m:mn>3</m:mn>
</m:mrow>
</m:mtd>
</m:mtr>
<m:mtr>
<m:mtd>
<m:mrow>
<m:mi>x</m:mi><m:mo>-</m:mo><m:mi>y</m:mi><m:mo>=</m:mo><m:mn>3</m:mn>

<exercise id="fs-id1165132960779"><problem id="fs-id1165132960780"><para id="fs-id1165137660236"> <m:math>
 <m:mrow>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:msup>
   <m:mi>x</m:mi>
   <m:mn>2 </m:mn>
  </m:msup>

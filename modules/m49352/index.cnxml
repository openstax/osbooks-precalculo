   <m:mn>3</m:mn>
  </m:mfrac>
  <m:mi>π</m:mi><m:msup>
   <m:mi>r</m:mi>
   <m:mn>2 </m:mn>
  </m:msup>
  <m:mi>h</m:mi><m:mo>.</m:mo>

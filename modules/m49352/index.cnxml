<solution id="fs-id1165137564896"><para id="fs-id1165137564897"> <m:math>
 <m:mrow>
  <m:msup>
   <m:mi>f</m:mi>
   <m:mrow>
    <m:mo>−</m:mo><m:mn>1</m:mn>
   </m:mrow>
  </m:msup>
  <m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:msqrt>
   <m:mrow>
    <m:mi>x</m:mi><m:mo>+</m:mo><m:mfrac>
     <m:mrow>
      <m:msup>
       <m:mi>b</m:mi>
       <m:mn>2 </m:mn>
      </m:msup>

     </m:mrow>
     <m:mn>4</m:mn>
    </m:mfrac>

   </m:mrow>
  </m:msqrt>
  <m:mo>−</m:mo><m:mfrac>
   <m:mi>b</m:mi>
   <m:mn>2 </m:mn>
  </m:mfrac>

 </m:mrow>
</m:math>
</para></solution></exercise>

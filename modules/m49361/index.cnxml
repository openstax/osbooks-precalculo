 <m:mrow>
  <m:mi>f</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mn>2000</m:mn><m:msup>
   <m:mrow>
    <m:mo stretchy="false">(</m:mo><m:mn>0,1</m:mn><m:mo stretchy="false">)</m:mo>
   </m:mrow>
   <m:mi>x</m:mi>
  </m:msup>

 </m:mrow>
</m:math>
</para></solution>
</exercise>

